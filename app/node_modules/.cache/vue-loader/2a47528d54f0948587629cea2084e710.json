cture(this.cameraOptions).then((imageData) => {\n        const base64Image = 'data:image/jpeg;base64,' + imageData;\n        alert(base64Image);\n      }).catch((err) => {\n        alert(err);\n      })\n    },\n  },\n});\n</script>\n\n<style>\n@import \"../styles/style.css\";\n</style>"]}]}