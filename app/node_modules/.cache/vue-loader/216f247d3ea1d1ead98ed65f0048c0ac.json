card-header>\n        <ion-card-content>\n          <div class=\"container\">\n            <img :src=\"img\" style=\"height: 25vh\" v-if=\"dataON\" />\n            <ion-skeleton-text\n              animated\n              v-if=\"!dataON\"\n              style=\"height: 25vh\"\n            ></ion-skeleton-text>\n\n            <ion-card\n              class=\"btnCard6 ion-activatable\"\n              @click=\"openCamera\"\n              color=\"cyan\"\n            >\n              <ion-card-content>\n                <ion-ripple-effect></ion-ripple-effect>\n                <h1 class=\"btnLabels2 useLondrina\">Reintentar</h1>\n              </ion-card-content>\n            </ion-card>\n          </div>\n        </ion-card-content>\n      </ion-card>\n\n      <ion-card>\n        <ion-card-header>\n          <ion-card-subtitle>Resultados del análisis </ion-card-subtitle>\n        </ion-card-header>\n        <ion-card-content>\n          <ion-grid>\n            \n            <ion-row class=\"residu6\" v-if=\"!dataON\">\n              <ion-row class=\"residu9\">\n                <ion-col class=\"container\" style=\"max-width: 4rem\">\n                  <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu3 useSora\"\n                  ></ion-skeleton-text>\n                </ion-col>\n                <ion-col>\n                  <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu4\"\n                     style=\"height:10vh\"\n                  ></ion-skeleton-text>\n                </ion-col>\n                <ion-col class=\"container\">\n                 <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu5\"\n                  ></ion-skeleton-text>\n                  <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu8\"\n                  ></ion-skeleton-text>\n                </ion-col>\n              </ion-row>\n            </ion-row>\n\n            <ion-row class=\"residu6\" v-if=\"!dataON\">\n              <ion-row class=\"residu9\">\n                <ion-col class=\"container\" style=\"max-width: 4rem\">\n                  <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu3 useSora\"\n                  ></ion-skeleton-text>\n                </ion-col>\n                <ion-col>\n                  <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu4\"\n                     style=\"height:10vh\"\n                  ></ion-skeleton-text>\n                </ion-col>\n                <ion-col class=\"container\">\n                 <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu5\"\n                  ></ion-skeleton-text>\n                  <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu8\"\n                  ></ion-skeleton-text>\n                </ion-col>\n              </ion-row>\n            </ion-row>\n\n            <ion-row class=\"residu6\" v-if=\"!dataON\">\n              <ion-row class=\"residu9\">\n                <ion-col class=\"container\" style=\"max-width: 4rem\">\n                  <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu3 useSora\"\n                  ></ion-skeleton-text>\n                </ion-col>\n                <ion-col>\n                  <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu4\"\n                    style=\"height:10vh\"\n                  ></ion-skeleton-text>\n                </ion-col>\n                <ion-col class=\"container\">\n                 <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu5\"\n                  ></ion-skeleton-text>\n                  <ion-skeleton-text\n                    animated\n                    v-if=\"!dataON\"\n                    class=\"residu8\"\n                  ></ion-skeleton-text>\n                </ion-col>\n              </ion-row>\n            </ion-row>\n\n            <ion-row\n              class=\"residu6\"\n              v-for=\"(item, idx) in results\"\n              :key=\"item.residuID\"\n            >\n              <ion-row class=\"residu9\">\n                <ion-col class=\"container\" style=\"max-width: 4rem\">\n                  <p class=\"residu3 useSora\">{{ idx + 1 }}</p>\n                </ion-col>\n                <ion-col class=\"container\">\n                  <img :src=\"item.img\" class=\"residu4\" />\n                </ion-col>\n                <ion-col class=\"container\">\n                  <p class=\"residu5\">{{ item.title }}</p>\n                  <p class=\"residu8\">{{ item.prob }}</p>\n                </ion-col>\n              </ion-row>\n\n              <ion-row class=\"container2 residu8\">\n                <ion-chip color=\"blue\" @click=\"openItem(item.residuID)\">\n                  <ion-icon\n                    :icon=\"checkmarkCircleOutline\"\n                    size=\"large\"\n                  ></ion-icon>\n                  <ion-label>Marcar como correcto</ion-label>\n                </ion-chip>\n              </ion-row>\n            </ion-row>\n          </ion-grid>\n        </ion-card-content>\n      </ion-card>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonContent,\n  IonHeader,\n  IonPage,\n  IonTitle,\n  IonToolbar,\n  IonButtons,\n  IonBackButton,\n  IonCard,\n  IonCardHeader,\n  IonCardSubtitle,\n  IonCardContent,\n  IonGrid,\n  IonRow,\n  IonCol,\n  IonIcon,\n  IonLabel,\n  IonChip,\n  IonRippleEffect,\n  loadingController,\n  IonSkeletonText,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\nimport { NativeStorage } from \"@ionic-native/native-storage\";\n\nimport { checkmarkCircleOutline } from \"ionicons/icons\";\n\nimport { Camera } from \"@ionic-native/camera\";\nimport { FileTransfer } from \"@ionic-native/file-transfer\";\n\nimport { useRouter } from \"vue-router\";\n\nexport default defineComponent({\n  name: \"VisionAIScannerPage\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonButtons,\n    IonBackButton,\n    IonCard,\n    IonCardHeader,\n    IonCardSubtitle,\n    IonCardContent,\n    IonGrid,\n    IonRow,\n    IonCol,\n    IonIcon,\n    IonLabel,\n    IonChip,\n    IonRippleEffect,\n    IonSkeletonText,\n  },\n  data: function () {\n    return {\n      messages: {\n        es: require(\"../../public/assets/i18n/es.json\"),\n        ca: require(\"../../public/assets/i18n/ca.json\"),\n        en: require(\"../../public/assets/i18n/en.json\"),\n      },\n      idioma: \"es\",\n      products: require(\"../../public/assets/products.json\"),\n      cameraOptions: {\n        quality: 100,\n        destinationType: Camera.DestinationType.FILE_URI,\n        sourceType: Camera.PictureSourceType.CAMERA,\n        mediaType: Camera.MediaType.PICTURE,\n        encodingType: Camera.EncodingType.JPEG,\n        correctOrientation: true, //Corrects Android orientation quirks\n      },\n      fileOptions: {\n        fileKey: \"myImage\",\n        fileName: \"file\",\n        mimeType: \"image/jpeg\",\n      },\n      img: null,\n      results: null,\n      loadingData: null,\n      dataON: false,\n    };\n  },\n  mounted: function () {\n    NativeStorage.getItem(\"idioma\").then((x) => (this.idioma = x));\n\n    this.openCamera();\n  },\n  methods: {\n    openCamera: function () {\n      loadingController\n        .create({\n          message: \"Analizando imagen...\",\n        })\n        .then((loading) => {\n          this.loadingData = loading;\n          loading.present();\n          Camera.getPicture(this.cameraOptions)\n            .then((imageData) => {\n              const fileTransfer = FileTransfer.create();\n\n              fileTransfer\n                .upload(\n                  imageData,\n                  encodeURI(\"http://192.168.1.41:3013/uploadImage\"),\n                  this.fileOptions\n                )\n                .then((data) => {\n                  const response = JSON.parse(data.response);\n\n                  this.img = \"http://192.168.1.41:3013\" + response[\"pathIMG\"];\n\n                  this.results = response[\"results\"];\n\n                  for (let i = 0; i < this.results.length; i++) {\n                    const id = this.results[i][\"residuID\"];\n                    this.results[i][\n                      \"img\"\n                    ] = require(\"../../public/assets/images/residus/\" +\n                      id +\n                      \"/\" +\n                      id +\n                      \"-1.jpg\");\n                    this.results[i][\"title\"] = this.products[id][\"nameES\"];\n                    this.results[i][\"prob\"] =\n                      Math.round(\n                        parseFloat(this.results[i][\"prob\"] * 100)\n                      ).toString() + \"%\";\n                  }\n\n                  this.dataON = true;\n\n                  this.loadingData.dismiss();\n                })\n                .catch((err) => {\n                  alert(\"Error: \" + err.toString());\n                  this.loadingData.dismiss();\n                });\n            })\n            .catch((err) => {\n              alert(\"Error: \" + err.toString());\n              this.loadingData.dismiss();\n              this.goBack()\n            });\n        })\n        .catch((err) => {\n          this.loadingData.dismiss();\n          alert(\"Error: \" + err.toString());\n        });\n    },\n  },\n  setup: function () {\n    const router = useRouter();\n\n        const goBack = () => {\n      router.go(-1);\n    };\n\n    const openItem = (p) =>{\n      router.push({ name: \"residu\", params: { residuID: p , filtered: \"false\"} });\n    }\n\n    return {\n      openItem,\n      goBack,\n      checkmarkCircleOutline,\n    };\n  },\n});\n</script>\n\n<style>\n@import \"../styles/style.css\";\n</style>"]}]}