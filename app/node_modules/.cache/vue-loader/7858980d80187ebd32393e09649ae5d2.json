E,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC;gBAC9B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;cAC5B,CAAC,CAAC;UACN,CAAC;UACD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE;YACd,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC;YAC9B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UAC5B,CAAC,CAAC;MACN,CAAC,CAAC;EACN,CAAC;AACH,CAAC,CAAC","file":"/Volumes/DATA SHARE/Trabajos 4t-UAB/TFG/Projecte/ecoAction/src/components/SendPage.vue","sourceRoot":"","sourcesContent":["<template>\n  <ion-page>\n    <ion-header :translucent=\"true\">\n      <ion-toolbar>\n        <IonButtons>\n          <ion-back-button :text=\"messages[idioma]['backText']\" />\n        </IonButtons>\n        <ion-title>Enviar tokens</ion-title>\n      </ion-toolbar>\n    </ion-header>\n\n    <ion-content :fullscreen=\"true\" class=\"ion-padding\">\n      <ion-card id=\"sendCard\">\n        <ion-card-header>\n          <ion-card-subtitle>Cantidad</ion-card-subtitle>\n        </ion-card-header>\n        <ion-card-content>\n          <ion-grid>\n            <ion-row style=\"margin-bottom: 2vh\">\n              <ion-col>\n                <ion-segment v-model=\"segmentValue\">\n                  <ion-segment-button value=\"tokens\">\n                    <ion-label>Tokens</ion-label>\n                  </ion-segment-button>\n                  <ion-segment-button value=\"fiat\">\n                    <ion-label>Fiat</ion-label>\n                  </ion-segment-button>\n                </ion-segment>\n              </ion-col>\n            </ion-row>\n            <ion-row>\n              <ion-col class=\"sendCol\">\n                <ion-input\n                  type=\"number\"\n                  v-model=\"tokenInput\"\n                  :disabled=\"disableTokens\"\n                ></ion-input>\n              </ion-col>\n              <ion-col class=\"midCol\"> ---- </ion-col>\n              <ion-col class=\"sendCol\">\n                <ion-input\n                  type=\"number\"\n                  v-model=\"fiatInput\"\n                  :disabled=\"disableFiat\"\n                ></ion-input>\n              </ion-col>\n            </ion-row>\n            <ion-row>\n              <ion-col class=\"midCol\">\n                <p>\n                  Comision fee: {{ comission }} {{ currency.toUpperCase() }}\n                </p>\n              </ion-col>\n            </ion-row>\n            <ion-row>\n              <ion-col class=\"midCol\">\n                <p>Total a pagar: {{ total }} {{ currency.toUpperCase() }}</p>\n              </ion-col>\n            </ion-row>\n          </ion-grid>\n        </ion-card-content>\n      </ion-card>\n\n      <ion-card>\n        <ion-card-header>\n          <ion-card-subtitle>Destinatario</ion-card-subtitle>\n        </ion-card-header>\n        <ion-card-content>\n          <ion-grid>\n            <ion-row>\n              <ion-col class=\"midCol\">\n                <p>Inserte identificador del usuario:</p>\n              </ion-col>\n            </ion-row>\n            <ion-row>\n              <ion-col class=\"midCol sendCol\">\n                <ion-input type=\"text\" v-model=\"identifier\"></ion-input>\n              </ion-col>\n            </ion-row>\n          </ion-grid>\n        </ion-card-content>\n      </ion-card>\n\n      <div class=\"container2\">\n        <div id=\"loginRegisterDiv\">\n          <ion-grid>\n            <ion-row>\n              <ion-col>\n                <ion-card\n                  class=\"btnCard3 ion-activatable\"\n                  color=\"warning\"\n                  @click=\"checkData\"\n                >\n                  <ion-card-content>\n                    <ion-ripple-effect></ion-ripple-effect>\n                    <h1 class=\"btnLabels2 useLondrina\">Verificar</h1>\n                  </ion-card-content>\n                </ion-card>\n              </ion-col>\n              <ion-col>\n                <ion-card\n                  class=\"btnCard3 ion-activatable\"\n                  color=\"lightgreen\"\n                  :disabled=\"!verified\"\n                  @click=\"updateTokens\"\n                >\n                  <ion-card-content>\n                    <ion-ripple-effect></ion-ripple-effect>\n                    <h1 class=\"btnLabels2 useLondrina\">Confirmar</h1>\n                  </ion-card-content>\n                </ion-card>\n              </ion-col>\n            </ion-row>\n          </ion-grid>\n        </div>\n      </div>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonContent,\n  IonHeader,\n  IonPage,\n  IonTitle,\n  IonToolbar,\n  IonButtons,\n  IonBackButton,\n  IonGrid,\n  IonRow,\n  IonCol,\n  IonCard,\n  IonCardContent,\n  IonInput,\n  IonSegment,\n  loadingController,\n  IonLabel,\n  IonCardHeader,\n  IonCardSubtitle,\n  IonRippleEffect,\n  IonSegmentButton,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\nimport { NativeStorage } from \"@ionic-native/native-storage\";\nimport axios from \"axios\";\nimport {sha224} from \"js-sha256\";\n\nexport default defineComponent({\n  name: \"SendPage\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonButtons,\n    IonBackButton,\n    IonGrid,\n    IonRow,\n    IonCol,\n    IonCard,\n    IonCardContent,\n    IonInput,\n    IonSegment,\n    IonLabel,\n    IonCardHeader,\n    IonRippleEffect,\n    IonCardSubtitle,\n    IonSegmentButton,\n  },\n  props: ['b', 't', 'm'],\n  data: function () {\n    return {\n      messages: {\n        es: require(\"../../public/assets/i18n/es.json\"),\n        ca: require(\"../../public/assets/i18n/ca.json\"),\n        en: require(\"../../public/assets/i18n/en.json\"),\n      },\n      idioma: \"es\",\n      currency: \"eur\",\n      tokenInput: 0,\n      fiatInput: 0,\n      changePrice: \"\",\n      controlInfo: null,\n      controlSend:null,\n      segmentValue: \"tokens\",\n      disableTokens: false,\n      disableFiat: true,\n      identifier: \"\",\n      comission: \"0\",\n      total: \"0\",\n      verified: false,\n      correctInput: true,\n      balance: 0,\n      checker: [\n        \"T\",\n        \"R\",\n        \"W\",\n        \"A\",\n        \"G\",\n        \"M\",\n        \"Y\",\n        \"F\",\n        \"P\",\n        \"D\",\n        \"X\",\n        \"B\",\n        \"N\",\n        \"J\",\n        \"Z\",\n        \"S\",\n        \"Q\",\n        \"V\",\n        \"H\",\n        \"L\",\n        \"C\",\n        \"K\",\n        \"E\",\n      ],\n    };\n  },\n  methods: {\n    checkIdentifier: function (id) {\n      const num = parseInt(id.substring(0, id.length - 1));\n      const letter = id.substring(id.length - 1, id.length);\n\n      const r = num % 23;\n\n      return (\n        this.checker[r] == letter.toLowerCase() ||\n        this.checker[r] == letter.toUpperCase()\n      );\n    },\n    checkData: function () {\n      if (this.correctInput) {\n        if (this.checkIdentifier(this.identifier)) {\n          axios({\n            url: \"http://192.168.1.41:3013/checkID?id=\" + this.identifier,\n            method: \"get\",\n          })\n            .then((response) => {\n              if (response.data == true) {\n                this.verified = true;\n              }\n            })\n            .catch((err) => {\n              console.log(\"Error => \" + err);\n            });\n        } else {\n          alert(\"Error: DNI inválido\");\n        }\n      }\n    },\n    updateTokens: function () {\n      const pass = prompt(\n        \"Introduzca su contraseña para autorizar el envio de tokens\"\n      );\n\n      if (pass != null && pass != \"\") {\n        loadingController\n          .create({\n            message:\n              \"Añadiendo tokens. Por favor espere y no cierre la aplicación\",\n          })\n          .then((loading) => {\n            this.controlSend = loading;\n            loading.present();\n            axios({\n              url: \"http://192.168.1.41:3013/updateTokens\",\n              method: \"post\",\n              data: [\n                this.identifier,\n                this.m,\n                sha224(pass),\n                this.tokenInput,\n              ],\n            }).then((response) => {\n              this.controlSend.dismiss();\n              alert(response.data);\n            });\n          })\n          .catch((err) => {\n            this.controlSend.dismiss();\n            alert(\"Se ha producido un error: \" + err);\n          });\n      }\n    },\n  },\n  watch: {\n    segmentValue: function () {\n      if (this.segmentValue == \"fiat\") {\n        this.disableTokens = true;\n        this.disableFiat = false;\n      } else {\n        this.disableTokens = false;\n        this.disableFiat = true;\n      }\n    },\n    tokenInput: function () {\n      if (this.disableFiat) {\n        this.fiatInput =\n          Math.round(this.tokenInput * 0.00001 * this.changePrice * 100) / 100;\n      }\n\n      if (this.tokenInput != \"\") {\n        if (parseFloat(this.tokenInput) <= parseFloat(this.balance)) {\n          axios({\n            url:\n              \"http://192.168.1.41:3013/getGasEstimation?method=send&addr=\" + this.t + \"&q=\" +\n              this.tokenInput +\n              \"&currency=\" +\n              this.currency,\n            method: \"get\",\n          })\n            .then((response2) => {\n              this.comission = response2.data;\n              this.total =\n                parseFloat(this.fiatInput) + parseFloat(this.comission);\n            })\n            .catch((err) => {\n              console.log(\"Error => \" + err);\n            });\n        } else {\n          alert(\"Insufficient balance\");\n        }\n      }\n    },\n    fiatInput: function () {\n      if (this.disableTokens) {\n        this.tokenInput = Math.round(\n          this.fiatInput / this.changePrice / 0.00001\n        );\n      }\n    },\n  },\n  mounted: async function () {\n    NativeStorage.getItem(\"idioma\").then((x) => (this.idioma = x));\n    await NativeStorage.getItem(\"currency\").then((x) => (this.currency = x));\n\n    loadingController\n      .create({\n        message: \"Cargando información...\",\n      })\n      .then((loading) => {\n        this.controlInfo = loading;\n        loading.present();\n        axios({\n          url: \"http://192.168.1.41:3013/getEthPrice?currency=\" + this.currency,\n          method: \"get\",\n        })\n          .then((response) => {\n            this.changePrice = parseFloat(response.data);\n\n            axios({\n              url:\n                \"http://192.168.1.41:3013/getGasEstimation?method=send&addr=\" + this.t + \"&q=\" +\n                this.tokenInput +\n                \"&currency=\" +\n                this.currency,\n              method: \"get\",\n            })\n              .then((response2) => {\n                this.comission = response2.data;\n                this.total =\n                  parseFloat(this.fiatInput) + parseFloat(this.comission);\n\n                axios({\n                  url:\n                    \"http://192.168.1.41:3013/getBalance?tokenAddress=\" + this.t +\n                    \"&currency=\" +\n                    this.currency,\n                  method: \"get\",\n                })\n                  .then((response3) => {\n                    this.balance = response3.data[0];\n                    this.controlInfo.dismiss();\n                  })\n                  .catch((err) => {\n                    console.log(\"Error => \" + err);\n                    this.controlInfo.dismiss();\n                  });\n              })\n              .catch((err) => {\n                console.log(\"Error => \" + err);\n                this.controlInfo.dismiss();\n              });\n          })\n          .catch((err) => {\n            console.log(\"Error => \" + err);\n            this.controlInfo.dismiss();\n          });\n      });\n  },\n});\n</script>\n\n<style scoped>\n@import \"../styles/style.css\";\n@import \"../styles/fontStyle.css\";\n@import \"../styles/colorStyle.css\";\n</style>"]}]}