     </IonButtons>\n        <ion-title>{{ messages[idioma][\"adminPage\"] }}</ion-title>\n      </ion-toolbar>\n    </ion-header>\n    <ion-content :fullscreen=\"true\">\n      <ion-header collapse=\"condense\">\n        <ion-toolbar>\n          <ion-title size=\"large\">{{\n            messages[idioma][\"adminPage\"]\n          }}</ion-title>\n        </ion-toolbar>\n      </ion-header>\n\n      <ion-grid>\n        <ion-row>\n          <ion-col>\n            <ion-card\n              class=\"btnCard ion-activatable\"\n              @click=\"openTokenUpdate(adminNum)\"\n              color=\"lightgreen\"\n            >\n              <ion-card-content>\n                <ion-ripple-effect></ion-ripple-effect>\n                <div class=\"container\">\n                  <ion-icon :icon=\"addCircle\" size=\"large\"></ion-icon>\n                  <h1 class=\"btnLabels\">Tokens</h1>\n                </div>\n              </ion-card-content>\n            </ion-card>\n          </ion-col>\n        </ion-row>\n      </ion-grid>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonPage,\n  IonHeader,\n  IonToolbar,\n  IonTitle,\n  IonContent,\n  IonButtons,\n  IonBackButton,\n  IonGrid,\n  IonRow,\n  IonCol,\n  IonCard,\n  IonCardContent,\n  IonIcon,\n  IonRippleEffect,\n} from \"@ionic/vue\";\n\nimport { NativeStorage } from \"@ionic-native/native-storage\";\nimport { addCircle } from \"ionicons/icons\";\nimport { useRouter } from \"vue-router\";\n\n\nexport default {\n  name: \"AdminFragment\",\n  components: {\n    IonHeader,\n    IonToolbar,\n    IonTitle,\n    IonContent,\n    IonPage,\n    IonButtons,\n    IonBackButton,\n    IonGrid,\n    IonRow,\n    IonCol,\n    IonCard,\n    IonCardContent,\n    IonIcon,\n    IonRippleEffect,\n  },\n  props:['adminNum'],\n  data: function () {\n    return {\n      messages: {\n        es: require(\"../../public/assets/i18n/es.json\"),\n        ca: require(\"../../public/assets/i18n/ca.json\"),\n        en: require(\"../../public/assets/i18n/en.json\"),\n      },\n      idioma: \"es\",\n    };\n  },\n  mounted: function () {\n    NativeStorage.getItem(\"idioma\").then((x) => {\n      this.idioma = x;\n    });\n  },\n  setup: function () {\n    const router = useRouter();\n\n     const openTokenUpdate = (id) => {\n      router.push({ name: \"tokensUpdate\", params: { adminNum: id } });\n    };\n\n    return {\n      addCircle,\n      openTokenUpdate\n    };\n  },\n};\n</script>\n\n<style>\n@import \"../styles/style.css\";\n@import \"../styles/colorStyle.css\";\n</style>"]}]}