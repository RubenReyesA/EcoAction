<ion-row>\n          <ion-col>\n            <div class=\"container\">\n              <ion-button @click=\"openRecyclingPoints\" size=\"large\">\n                <ion-icon :icon=\"leaf\" size=\"large\"></ion-icon>\n                <ion-label class=\"btnLabels\">Deixalleries</ion-label>\n              </ion-button>\n            </div>\n          </ion-col>\n        </ion-row>\n        <ion-row>\n          <ion-col>\n            <div class=\"container\">\n              <ion-button @click=\"openWhyRecycle\" size=\"large\">\n                <ion-icon :icon=\"helpCircle\" size=\"large\"></ion-icon>\n                <ion-label class=\"btnLabels\">Porque reciclar?</ion-label>\n              </ion-button>\n            </div>\n          </ion-col>\n        </ion-row>\n        <ion-row>\n          <ion-col>\n            <div class=\"container\">\n              <ion-button @click=\"openWaste\" size=\"large\">\n                <ion-icon :icon=\"cube\" size=\"large\"></ion-icon>\n                <ion-label class=\"btnLabels\">Residus</ion-label>\n              </ion-button>\n            </div>\n          </ion-col>\n        </ion-row>\n        <ion-row>\n          <ion-col>\n            <div class=\"container\">\n              <ion-button @click=\"openContainers\" size=\"large\">\n                <ion-icon :icon=\"trashBin\" size=\"large\"></ion-icon>\n                <ion-label class=\"btnLabels\">Contenidors</ion-label>\n              </ion-button>\n            </div>\n          </ion-col>\n        </ion-row>\n        <ion-row>\n          <ion-col>\n            <div class=\"container\">\n              <ion-button @click=\"openAwards\" size=\"large\">\n                <ion-icon :icon=\"trophy\" size=\"large\"></ion-icon>\n                <ion-label class=\"btnLabels\">Recompensas</ion-label>\n              </ion-button>\n            </div>\n          </ion-col>\n        </ion-row>\n      </ion-grid>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonPage,\n  IonHeader,\n  IonToolbar,\n  IonTitle,\n  IonContent,\n  IonLabel,\n  IonButton,\n  IonIcon,\n  IonGrid,\n  IonCol,\n  IonRow,\n} from \"@ionic/vue\";\n\nimport { leaf, helpCircle, cube, trashBin, trophy } from \"ionicons/icons\";\nimport { defineComponent } from \"vue\";\nimport { useRouter } from \"vue-router\";\n\nexport default defineComponent({\n  name: \"InfoFragment\",\n  components: {\n    IonHeader,\n    IonToolbar,\n    IonTitle,\n    IonContent,\n    IonPage,\n    IonLabel,\n    IonButton,\n    IonIcon,\n    IonGrid,\n    IonCol,\n    IonRow,\n  },\n  setup: function () {\n    const router = useRouter();\n\n    const openRecyclingPoints = () => {\n      router.push(\"/main/info/recyclingPoints\");\n    };\n\n    const openWhyRecycle = () => {\n      router.push(\"/main/info/whyReciclying\");\n    };\n\n    const openWaste = () => {\n      router.push(\"/main/info/waste\");\n    };\n\n    const openContainers = () => {\n      router.push(\"/main/info/containers\");\n    };\n\n    const openAwards = () => {\n      router.push(\"/main/info/awards\");\n    };\n\n    return {\n      openRecyclingPoints,\n      openWhyRecycle,\n      openWaste,\n      openContainers,\n      openAwards,\n      leaf,\n      helpCircle,\n      cube,\n      trashBin,\n      trophy,\n    };\n  },\n});\n</script>\n\n<style>\n@import \"../styles/infoStyle.css\";\n</style>"]}]}