,\n  created: function () {\n    this.checkLogin();\n    EventBus.on(\"hi\", this.checkLogin);\n  },\n  setup: function () {\n    return {\n      logIn,\n      refreshCircle,\n    };\n  },\n  methods: {\n    getBalances: function () {\n      const addr = \"0x2951684B0A8DD4b655bF5924be9e351f3d3360e2\";\n      axios({\n        url: \"http://192.168.1.41:3013/getBalance?tokenAddress=\" + addr,\n        method: \"get\",\n      }).then((response) => {\n        loadingController\n          .create({\n            message: \"Cargando saldo...\",\n          })\n          .then((loading) => {\n            this.control = loading;\n            loading.present();\n            setTimeout(() => {\n              this.user.balance = response.data + \"\";\n              this.control.dismiss();\n            }, 1000);\n          });\n      });\n    },\n    checkLogin: function () {\n      NativeStorage.getItem(\"login\")\n        .then((res) => {\n          this.user.name = res;\n          this.showUser = true;\n          this.updateInfoUser();\n        })\n        .catch(() => {\n          this.showUser = false;\n          this.user = {\n            name: \"\",\n            balance: \"0\",\n          };\n        });\n    },\n    logOut: function () {\n      NativeStorage.remove(\"login\");\n      alert(\"Sesión cerrada correctamente\");\n      this.checkLogin();\n    },\n    updateInfoUser: function () {\n      this.getBalances();\n    },\n    storage: function () {\n      NativeStorage.clear().then(() => alert(\"Content erased!\"));\n    },\n    photo: function () {\n      Camera.getPicture(this.optionsCamera)\n        .then((imageData) => {\n          // imageData is either a base64 encoded string or a file URI\n          // If it's base64 (DATA_URL):\n\n          const f = FileTransfer.create();\n\n          f.upload(imageData, \"http://192.168.1.41:2999/upload\", {\n            fileKey: \"file\",\n            fileName: \"test.jpg\",\n          }).then((data) => {\n            alert(\"Success: \" + data);\n          });\n        })\n        .catch((err) => {\n          alert(\"Error: \" + err);\n        });\n    },\n    login: function () {\n      FingerprintAIO.show({\n        title: \"Login\",\n        subtitle: \"Please login to connect to the servers\",\n        description: \"This is authentication\",\n      })\n        .then((result) => {\n          alert(result + \" OK!\");\n        })\n        .catch((err) => {\n          alert(err + \" ERROR!\");\n        });\n    },\n    loginsaver: function () {\n      KeychainTouchId.isAvailable()\n        .then((res) => {\n          const pass = \"123123\";\n\n          KeychainTouchId.save(\"user1\", pass, true)\n            .then((res2) => {\n              alert(\"Login info saved\" + res + \"-\" + res2);\n            })\n            .catch((err2) => {\n              alert(\"Error: \" + err2);\n            });\n        })\n        .catch((err) => alert(\"Error: \" + err));\n    },\n    loginverifier: function () {\n      KeychainTouchId.isAvailable()\n        .then(() => {\n          KeychainTouchId.verify(\"user1\", \"Use to unlock app\")\n            .then((res2) => {\n              alert(\"User: user1 and Password: \" + res2);\n            })\n            .catch((err2) => {\n              alert(\"Error: \" + err2);\n            });\n        })\n        .catch((err) => alert(\"Error: \" + err));\n    },\n    barscanner: function () {\n      BarcodeScanner.scan()\n        .then((barcodeData) => {\n          //alert(\"Barcode data: \" + barcodeData.text);\n          this.code = barcodeData.text;\n\n          axios({\n            url: \"http://192.168.1.41:3013/code?code=\" + this.code,\n            method: \"get\",\n          }).then((response) => {\n            this.source = response.data[2];\n            this.name = response.data[0];\n            this.content = response.data[1];\n          });\n        })\n        .catch((err) => {\n          alert(\"Error: \" + err);\n        });\n    },\n    test: function () {\n      this.code = \"8470001790590\";\n\n      axios({\n        url: \"http://192.168.1.41:3012/code?code=\" + this.code,\n        method: \"get\",\n      }).then((response) => {\n        this.source = response.data[0];\n        this.name = response.data[1].substring(0, 35) + \"...\";\n      });\n    },\n  },\n});\n</script>"]}]}