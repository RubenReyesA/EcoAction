         messages[idioma][\"mailTitle\"]\n            }}</ion-label>\n            <ion-input v-model=\"mail\" slot=\"end\" type=\"email\"></ion-input>\n          </ion-item>\n\n          <ion-item>\n            <ion-label class=\"ion-text-wrap\">{{\n              messages[idioma][\"passwordTitle\"]\n            }}</ion-label>\n            <ion-input\n              v-model=\"password\"\n              slot=\"end\"\n              type=\"password\"\n            ></ion-input>\n          </ion-item>\n\n          <ion-item>\n            <ion-label class=\"ion-text-wrap\">{{\n              messages[idioma][\"confirmPasswordTitle\"]\n            }}</ion-label>\n            <ion-input\n              v-model=\"password2\"\n              slot=\"end\"\n              type=\"password\"\n            ></ion-input>\n          </ion-item>\n        </ion-item-group>\n      </ion-list>\n      <ion-button class=\"lgRg_btn\" expand=\"block\" @click=\"startRegister\">\n        {{ messages[idioma][\"registerTitle\"] }}\n      </ion-button>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonContent,\n  IonHeader,\n  IonPage,\n  IonTitle,\n  IonToolbar,\n  IonButtons,\n  IonButton,\n  IonBackButton,\n  IonList,\n  IonLabel,\n  IonItem,\n  IonItemDivider,\n  IonItemGroup,\n  IonInput,\n  loadingController,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\nimport { NativeStorage } from \"@ionic-native/native-storage\";\n\nimport axios from \"axios\";\nimport { useRouter } from \"vue-router\";\nimport sha256 from \"js-sha256\";\n\nexport default defineComponent({\n  name: \"RegisterPage\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonButtons,\n    IonBackButton,\n    IonButton,\n    IonList,\n    IonLabel,\n    IonItem,\n    IonInput,\n    IonItemDivider,\n    IonItemGroup,\n  },\n  data: function () {\n    return {\n      messages: {\n        es: require(\"../../public/assets/i18n/es.json\"),\n        ca: require(\"../../public/assets/i18n/ca.json\"),\n        en: require(\"../../public/assets/i18n/en.json\"),\n      },\n      idioma: \"es\",\n      control: null,\n      name: null,\n      surname: null,\n      mail: null,\n      password: null,\n      password2: null,\n    };\n  },\n  setup: function(){\n    const router = useRouter();\n\n    const gotoWallet = (mail) => {\n      router.push('/main/home/register/seedGeneration?mail=' + mail);\n    };\n\n    return { gotoWallet };\n  },\n  mounted: function () {\n    NativeStorage.getItem(\"idioma\").then((x) => (this.idioma = x));\n  },\n  methods: {\n    checkInputs: function () {\n      //Check empty values\n      if (\n        this.name == \"\" ||\n        this.surname == \"\" ||\n        this.mail == \"\" ||\n        this.password == \"\" ||\n        this.password2 == \"\"\n      ) {\n        console.log(false);\n        return false;\n      }\n\n      //Check mail value\n      const re = /\\S+@\\S+\\.\\S+/;\n      if (!re.test(this.mail)) {\n        console.log(false);\n        return false;\n      }\n\n      //Check if passwords match\n\n      if (this.password != this.password2) {\n        console.log(false);\n        return false;\n      }\n\n      //Check password structure and length\n      if (\n        this.password.match(/[a-zA-Z]/g) &&\n        this.password.match(/[0-9]/g) &&\n        this.password.match(/[^a-zA-Z\\d]/g) &&\n        this.password.length >= 8\n      ) {\n        console.log(true);\n        return true;\n      } else {\n        console.log(false);\n        return false;\n      }\n    },\n    startRegister: function () {\n      if (this.checkInputs()) {\n        loadingController\n          .create({\n            message: \"Confirmando registro...\",\n          })\n          .then((loading) => {\n            this.control = loading;\n            loading.present();\n            axios({\n              url: \"http://192.168.1.41:3013/registerUser\",\n              method: \"post\",\n              headers: { Accept: \"application/json\" },\n              data: {\n                name: this.name,\n                surname: this.surname,\n                mail: this.mail,\n                password: sha256(this.password),\n              },\n            })\n              .then((response) => {\n                console.log(response.data);\n                if (response.data == 'OK') {\n                  this.control.dismiss();\n                  alert(\n                    \"Registro completado\"\n                  );\n                  this.gotoWallet(this.mail);\n                } else {\n                  this.control.dismiss();\n                  alert(\n                    response.data\n                  );\n                }\n              })\n              .catch((err) => {\n                this.control.dismiss();\n                alert(\"Se ha producido un error: \" + err);\n              });\n          });\n      }\n    },\n  },\n});\n</script>\n\n<style>\n@import \"../styles/homeStyle.css\";\n</style>"]}]}