-card>\n        <ion-card-header>\n          <ion-card-title>{{ title }}</ion-card-title>\n        </ion-card-header>\n\n        <ion-card-content>\n          <ion-slides pager=\"true\" :options=\"sliderOptions\" v-if=\"(count == 0)\">\n            <ion-slide\n              v-for=\"i in imgPathstoShow\"\n              :key=\"i\"\n              class=\"sliderCenter\"\n            >\n              <img\n                :src=\"i\"\n                class=\"imgSlider\"\n                style=\"width: 50vw; height: 30vh\"\n              />\n            </ion-slide>\n          </ion-slides>\n          <ion-slides pager=\"true\" :options=\"sliderOptions\" v-if=\"(count == 1)\">\n            <ion-slide\n              v-for=\"i in imgPathstoShow\"\n              :key=\"i\"\n              class=\"sliderCenter\"\n            >\n              <img\n                :src=\"i\"\n                class=\"imgSlider\"\n                style=\"width: 50vw; height: 30vh\"\n              />\n            </ion-slide>\n          </ion-slides>\n          <ion-slides pager=\"true\" :options=\"sliderOptions\" v-if=\"(count == 2)\">\n            <ion-slide\n              v-for=\"i in imgPathstoShow\"\n              :key=\"i\"\n              class=\"sliderCenter\"\n            >\n              <img\n                :src=\"i\"\n                class=\"imgSlider\"\n                style=\"width: 50vw; height: 30vh\"\n              />\n            </ion-slide>\n          </ion-slides>\n        </ion-card-content>\n      </ion-card>\n\n      <div class=\"container2\">\n        <ion-grid>\n          <ion-row>\n            <ion-col>\n              <ion-card\n                class=\"ion-activatable\"\n                @click=\"selectCorrect\"\n                color=\"success\"\n              >\n                <ion-card-content>\n                  <ion-ripple-effect></ion-ripple-effect>\n                  <h1 class=\"btnLabels2 useLondrina\">\n                    Seleccionar como correcto\n                  </h1>\n                </ion-card-content>\n              </ion-card>\n            </ion-col>\n          </ion-row>\n\n          <ion-row>\n            <ion-col>\n              <ion-card\n                class=\"ion-activatable\"\n                @click=\"next\"\n                color=\"warning\"\n                v-if=\"!istheLast\"\n              >\n                <ion-card-content>\n                  <ion-ripple-effect></ion-ripple-effect>\n                  <h1 class=\"btnLabels2 useLondrina\">Siguiente intento</h1>\n                </ion-card-content>\n              </ion-card>\n              <ion-card\n                class=\"ion-activatable\"\n                @click=\"close\"\n                color=\"danger\"\n                v-if=\"istheLast\"\n              >\n                <ion-card-content>\n                  <ion-ripple-effect></ion-ripple-effect>\n                  <h1 class=\"btnLabels2 useLondrina\">Cerrar</h1>\n                </ion-card-content>\n              </ion-card>\n            </ion-col>\n          </ion-row>\n        </ion-grid>\n      </div>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonContent,\n  IonHeader,\n  IonPage,\n  IonTitle,\n  IonButtons,\n  IonButton,\n  IonToolbar,\n  IonCard,\n  IonCardContent,\n  IonCardHeader,\n  IonCardTitle,\n  IonRippleEffect,\n  IonSlides,\n  IonSlide,\n  IonGrid,\n  IonRow,\n  IonCol,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\nimport EventBus from \"eventing-bus\";\n\nexport default defineComponent({\n  name: \"AIObjectModal\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonCard,\n    IonCardContent,\n    IonCardHeader,\n    IonRippleEffect,\n    IonCardTitle,\n    IonSlides,\n    IonSlide,\n    IonGrid,\n    IonRow,\n    IonCol,\n      IonButtons,\n  IonButton,\n  },\n  props: [\"ids\", \"titles\", \"imgPaths\", \"objectIds\"],\n  data: function () {\n    return {\n      id: null,\n      count: 0,\n      title: \"\",\n      imgPathstoShow: [],\n      istheLast: false,\n      sliderOptions: {\n        initialSlide: 0,\n        slidesPerView: 1,\n        autoplay: true,\n      },\n    };\n  },\n  watch: {\n    count: function () {\n      this.applyChanges();\n    },\n  },\n  methods: {\n    selectCorrect: function () {\n      EventBus.publish(\"closeModalAIwithSuccess\", this.objectIds[this.id - 1]);\n    },\n    next: function () {\n      this.count++;\n    },\n    close: function () {\n      this.dismissModal();\n    },\n    dismissModal: function () {\n      EventBus.publish(\"closeModalAI\");\n    },\n    applyChanges: function () {\n      this.id = this.ids[this.count];\n      this.title = this.titles[this.ids[this.count] - 1];\n\n      this.imgPathstoShow[0] = require(\"../../public/assets/images/residus/\" +\n        this.imgPaths[this.ids[this.count] - 1][0]);\n      this.imgPathstoShow[1] = require(\"../../public/assets/images/residus/\" +\n        this.imgPaths[this.ids[this.count] - 1][1]);\n      this.imgPathstoShow[2] = require(\"../../public/assets/images/residus/\" +\n        this.imgPaths[this.ids[this.count] - 1][2]);\n      this.imgPathstoShow[3] = require(\"../../public/assets/images/residus/\" +\n        this.imgPaths[this.ids[this.count] - 1][3]);\n\n      if (this.count == 2) {\n        this.istheLast = true;\n      }\n    },\n  },\n  mounted: function () {\n    this.applyChanges();\n  },\n});\n</script>\n\n<style>\n@import \"../styles/style.css\";\n</style>"]}]}