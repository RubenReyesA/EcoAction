olbar>\n      </ion-header>\n\n      <InfoUserSession price=\"12,55€\" v-if=\"showUser\" />\n\n      <LoginRegisterSession v-if=\"!showUser\" />\n\n      <ion-button @click=\"loginsaver\"> SAVER </ion-button>\n      <ion-button @click=\"loginverifier\"> VERIFIER </ion-button>\n      <ion-button @click=\"barscanner\"> SCANNER+ </ion-button>\n      <ion-button @click=\"photo\"> PHOTO+ </ion-button>\n      <ion-button @click=\"storage()\"> STORAGE CLEAR </ion-button>\n\n      <ion-card>\n        <img :src=\"source\" />\n        <ion-card-header>\n          <ion-card-subtitle>{{ code }}</ion-card-subtitle>\n          <ion-card-title>{{ name }}</ion-card-title>\n        </ion-card-header>\n\n        <ion-card-content>\n          {{ content }}\n        </ion-card-content>\n      </ion-card>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonCard,\n  IonPage,\n  IonHeader,\n  IonToolbar,\n  IonTitle,\n  IonContent,\n  IonCardContent,\n  IonCardHeader,\n  IonCardSubtitle,\n  IonCardTitle,\n  IonButton,\n} from \"@ionic/vue\";\n\nimport { FingerprintAIO } from \"@ionic-native/fingerprint-aio\";\nimport { BarcodeScanner } from \"@ionic-native/barcode-scanner\";\nimport { KeychainTouchId } from \"@ionic-native/keychain-touch-id\";\nimport { Camera } from \"@ionic-native/camera\";\n\nimport { FileTransfer } from \"@ionic-native/file-transfer\";\n\nimport axios from \"axios\";\nimport { NativeStorage } from \"@ionic-native/native-storage\";\n\nimport InfoUserSession from \"../components/InfoUserSession\";\nimport LoginRegisterSession from \"../components/LoginRegisterSession\";\n\nimport { defineComponent } from \"vue\";\n\nexport default defineComponent({\n  name: \"HomeFragment\",\n  components: {\n    IonCard,\n    IonPage,\n    IonHeader,\n    IonToolbar,\n    IonTitle,\n    IonContent,\n    IonCardContent,\n    IonCardHeader,\n    IonCardSubtitle,\n    IonCardTitle,\n    IonButton,\n    InfoUserSession,\n    LoginRegisterSession,\n  },\n  data: function () {\n    return {\n      source: \"\",\n      name: \"\",\n      showUser: true,\n      user: {\n        name: \"Rubén\",\n      },\n      code: \"\",\n      content: \"\",\n      optionsCamera: {\n        quality: 75,\n        destinationType: Camera.DestinationType.FILE_URI,\n        encodingType: Camera.EncodingType.JPEG,\n        mediaType: Camera.MediaType.PICTURE,\n      },\n    };\n  },\n  methods: {\n    storage: function () {\n      NativeStorage.clear().then(() => alert(\"Content erased!\"));\n    },\n    photo: function () {\n      Camera.getPicture(this.optionsCamera)\n        .then((imageData) => {\n          // imageData is either a base64 encoded string or a file URI\n          // If it's base64 (DATA_URL):\n\n          const f = FileTransfer.create();\n\n          f.upload(imageData, \"http://192.168.1.41:2999/upload\", {\n            fileKey: \"file\",\n            fileName: \"test.jpg\",\n          }).then((data) => {\n            alert(\"Success: \" + data);\n          });\n        })\n        .catch((err) => {\n          alert(\"Error: \" + err);\n        });\n    },\n    login: function () {\n      FingerprintAIO.show({\n        title: \"Login\",\n        subtitle: \"Please login to connect to the servers\",\n        description: \"This is authentication\",\n      })\n        .then((result) => {\n          alert(result + \" OK!\");\n        })\n        .catch((err) => {\n          alert(err + \" ERROR!\");\n        });\n    },\n    loginsaver: function () {\n      KeychainTouchId.isAvailable()\n        .then((res) => {\n          const pass = \"123123\";\n\n          KeychainTouchId.save(\"user1\", pass, true)\n            .then((res2) => {\n              alert(\"Login info saved\" + res + \"-\" + res2);\n            })\n            .catch((err2) => {\n              alert(\"Error: \" + err2);\n            });\n        })\n        .catch((err) => alert(\"Error: \" + err));\n    },\n    loginverifier: function () {\n      KeychainTouchId.isAvailable()\n        .then(() => {\n          KeychainTouchId.verify(\"user1\", \"Use to unlock app\")\n            .then((res2) => {\n              alert(\"User: user1 and Password: \" + res2);\n            })\n            .catch((err2) => {\n              alert(\"Error: \" + err2);\n            });\n        })\n        .catch((err) => alert(\"Error: \" + err));\n    },\n    barscanner: function () {\n      BarcodeScanner.scan()\n        .then((barcodeData) => {\n          //alert(\"Barcode data: \" + barcodeData.text);\n          this.code = barcodeData.text;\n\n          axios({\n            url: \"http://192.168.1.41:3013/code?code=\" + this.code,\n            method: \"get\",\n          }).then((response) => {\n            this.source = response.data[2];\n            this.name = response.data[0];\n            this.content = response.data[1];\n          });\n        })\n        .catch((err) => {\n          alert(\"Error: \" + err);\n        });\n    },\n    test: function () {\n      this.code = \"8470001790590\";\n\n      axios({\n        url: \"http://192.168.1.41:3012/code?code=\" + this.code,\n        method: \"get\",\n      }).then((response) => {\n        this.source = response.data[0];\n        this.name = response.data[1].substring(0, 35) + \"...\";\n      });\n    },\n  },\n});\n</script>"]}]}