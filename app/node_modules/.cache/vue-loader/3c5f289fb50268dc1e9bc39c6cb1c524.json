</ion-tab-button>\n\n        <ion-tab-button tab=\"settings\" href=\"/main/settings\">\n          <ion-icon :icon=\"cog\" />\n          <ion-label>Configuració</ion-label>\n        </ion-tab-button>\n      </ion-tab-bar>\n    </ion-tabs>\n  </ion-page>\n</template>\n\n<script>\nimport { IonTabBar, IonTabButton, IonTabs, IonLabel, IonIcon, IonPage, IonSlides, IonSlide, IonButton } from '@ionic/vue';\nimport { informationCircle, search, home, cog } from 'ionicons/icons';\nimport { NativeStorage } from '@ionic-native/native-storage';\n\nexport default {\n  name: 'Main',\n  components: { IonLabel, IonTabs, IonTabBar, IonTabButton, IonIcon, IonPage, IonSlides, IonSlide, IonButton },\n  data: function(){\n    return {\n      isSlides:null,\n      isTabs:null,\n    }\n  },\n  created: async function(){\n\n    NativeStorage.getItem(\"firstRun\")\n    .then(()=> {\n      this.isSlides=false;\n      this.isTabs=true;\n    })\n    .catch(()=> {\n      this.isSlides=true;\n      this.isTabs=false;\n      NativeStorage.setItem(\"firstRun\", \"DONE\");\n    });\n  },\n  setup() {\n    return {\n      informationCircle, \n      search, \n      home,\n      cog,\n    }\n  }\n}\n</script>\n<style>\n    ion-slides {\n      height: 100%;\n    }\n\n    .swiper-slide {\n      display: block;\n    }\n\n    .swiper-slide h2 {\n      margin-top: 2.8rem;\n    }\n\n    .swiper-slide img {\n      max-height: 50%;\n      max-width: 80%;\n      margin: 60px 0 40px;\n      pointer-events: none;\n    }\n\n    b {\n      font-weight: 500;\n    }\n\n    p {\n      padding: 0 40px;\n      font-size: 14px;\n      line-height: 1.5;\n      color: var(--ion-color-step-600, #60646b);\n    }\n\n    p b {\n      color: var(--ion-text-color, #000000);\n    }\n  </style>"]}]}