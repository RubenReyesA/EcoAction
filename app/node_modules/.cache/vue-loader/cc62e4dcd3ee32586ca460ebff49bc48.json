"true\">\n      <ion-toolbar>\n        <IonButtons>\n          <ion-back-button :text=\"messages[idioma]['backText']\" />\n        </IonButtons>\n        <ion-title>Recibir Tokens</ion-title>\n      </ion-toolbar>\n    </ion-header>\n\n    <ion-content :fullscreen=\"true\" class=\"ion-padding\">\n      <ion-card>\n        <ion-card-header>\n          <ion-card-title>Cómo recibir tokens?</ion-card-title>\n        </ion-card-header>\n        <ion-card-content>\n          <h2>Puedes recibir tokens a partir de logros o de algún amigo/a</h2>\n        </ion-card-content>\n      </ion-card>\n\n      <ion-card>\n        <ion-card-header>\n          <ion-card-subtitle>Instrucciones</ion-card-subtitle>\n        </ion-card-header>\n        <ion-card-content>\n          <ion-grid>\n            <ion-row class=\"residu6\">\n              <ion-col class=\"container\" style=\"max-width: 4rem\">\n                <p class=\"residu3 useSora\">1</p>\n              </ion-col>\n              <ion-col class=\"container\">\n                <p class=\"residu5\">\n                  Tu amigo/a debe iniciar sesión en su cuenta de EcoAction y\n                  hacer clic en \"Enviar\"\n                </p>\n              </ion-col>\n              <ion-col class=\"container\" style=\"max-width: 2rem\">\n                <ion-buttons>\n                  <ion-button @click=\"openInfo(1)\">\n                    <ion-icon\n                      slot=\"icon-only\"\n                      :icon=\"informationCircleOutline\"\n                    ></ion-icon>\n                  </ion-button>\n                </ion-buttons>\n              </ion-col>\n            </ion-row>\n            <ion-row class=\"residu6\">\n              <ion-col class=\"container\" style=\"max-width: 4rem\">\n                <p class=\"residu3 useSora\">2</p>\n              </ion-col>\n              <ion-col class=\"container\">\n                <p class=\"residu5\">\n                  Debe introducir la cantidad de EAs que desea enviar, teniendo\n                  en cuenta la comisión asociada\n                </p>\n              </ion-col>\n              <ion-col class=\"container\" style=\"max-width: 2rem\">\n                <ion-buttons>\n                  <ion-button @click=\"openInfo(2)\">\n                    <ion-icon\n                      slot=\"icon-only\"\n                      :icon=\"informationCircleOutline\"\n                    ></ion-icon>\n                  </ion-button>\n                </ion-buttons>\n              </ion-col>\n            </ion-row>\n            <ion-row class=\"residu6\">\n              <ion-col class=\"container\" style=\"max-width: 4rem\">\n                <p class=\"residu3 useSora\">3</p>\n              </ion-col>\n              <ion-col class=\"container\">\n                <p class=\"residu5\">\n                  Luego, debe introducir el siguiente identificador:\n                </p>\n                <h1 class=\"useSora\" style=\"margin-top:10px\"> 771xxxxxG</h1>\n              </ion-col>\n              <ion-col class=\"container\" style=\"max-width: 2rem\">\n                <ion-buttons>\n                  <ion-button @click=\"openInfo(3)\">\n                    <ion-icon\n                      slot=\"icon-only\"\n                      :icon=\"informationCircleOutline\"\n                    ></ion-icon>\n                  </ion-button>\n                </ion-buttons>\n              </ion-col>\n            </ion-row>\n            <ion-row class=\"residu6\">\n              <ion-col class=\"container\" style=\"max-width: 4rem\">\n                <p class=\"residu3 useSora\">4</p>\n              </ion-col>\n              <ion-col class=\"container\">\n                <p class=\"residu5\">\n                  Para finalizar, debe confirmar el envío y esperar hasta que la\n                  transacción se complete\n                </p>\n              </ion-col>\n              <ion-col class=\"container\" style=\"max-width: 2rem\">\n                <ion-buttons>\n                  <ion-button @click=\"openInfo(4)\">\n                    <ion-icon\n                      slot=\"icon-only\"\n                      :icon=\"informationCircleOutline\"\n                    ></ion-icon>\n                  </ion-button>\n                </ion-buttons>\n              </ion-col>\n            </ion-row>\n            <ion-row class=\"residu6\">\n              <ion-col class=\"container\" style=\"max-width: 4rem\">\n                <p class=\"residu3 useSora\">5</p>\n              </ion-col>\n              <ion-col class=\"container\">\n                <p class=\"residu5\">Listo! Ya has recibido tus tokens</p>\n              </ion-col>\n              <ion-col class=\"container\" style=\"max-width: 2rem\">\n                <ion-buttons>\n                  <ion-button @click=\"openInfo(5)\">\n                    <ion-icon\n                      slot=\"icon-only\"\n                      :icon=\"informationCircleOutline\"\n                    ></ion-icon>\n                  </ion-button>\n                </ion-buttons>\n              </ion-col>\n            </ion-row>\n          </ion-grid>\n        </ion-card-content>\n      </ion-card>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonContent,\n  IonHeader,\n  IonPage,\n  IonTitle,\n  IonToolbar,\n  IonButtons,\n  IonBackButton,\n  IonCardContent,\n  IonCard,\n  IonCardHeader,\n  IonCardTitle,\n  IonCardSubtitle,\n  IonGrid,\n  IonRow,\n  IonCol,\n  IonButton,\n  IonIcon,\n  alertController,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\nimport { NativeStorage } from \"@ionic-native/native-storage\";\n\nimport { informationCircleOutline } from \"ionicons/icons\";\n\nexport default defineComponent({\n  name: \"ReceivePage\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonButtons,\n    IonBackButton,\n    IonCardContent,\n    IonCard,\n    IonCardHeader,\n    IonCardTitle,\n    IonCardSubtitle,\n    IonGrid,\n    IonRow,\n    IonCol,\n    IonButton,\n    IonIcon,\n  },\n  data: function () {\n    return {\n      messages: {\n        es: require(\"../../public/assets/i18n/es.json\"),\n        ca: require(\"../../public/assets/i18n/ca.json\"),\n        en: require(\"../../public/assets/i18n/en.json\"),\n      },\n      idioma: \"es\",\n      img: null,\n    };\n  },\n  mounted: function () {\n    NativeStorage.getItem(\"idioma\").then((x) => (this.idioma = x));\n    this.img=[];\n\n    this.img[1] = require(\"../../public/assets/images/receive/part1.jpg\");\n    this.img[2] = require(\"../../public/assets/images/receive/part2.jpg\");\n    this.img[3] = require(\"../../public/assets/images/receive/part3.jpg\");\n    this.img[4] = require(\"../../public/assets/images/receive/part4.jpg\");\n    this.img[5] = require(\"../../public/assets/images/receive/part5.jpg\");\n\n  },\n  setup: function () {\n    return { informationCircleOutline };\n  },\n  methods: {\n    openInfo: async function (id) {\n      const alert = await alertController.create({\n        message: `<img src=\"${this.img[id]}\">`,\n        buttons: [\n          {\n            text: \"OK\",\n            role: \"cancel\",\n          },\n        ],\n      });\n\n      await alert.present();\n    },\n  },\n});\n</script>\n\n<style scoped>\n</style>"]}]}