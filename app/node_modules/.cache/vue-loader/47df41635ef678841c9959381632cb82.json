efault defineComponent({\n  name: \"seedGeneration\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonButton,\n    IonGrid,\n    IonRow,\n    IonCol,\n    IonLabel,\n  },\n  props: {\n    mail: String,\n  },\n  created: function () {\n    this.wordsList = [];\n\n    axios({\n      url: \"http://192.168.1.41:3013/generateNewWallet?mail=\" + this.mail,\n      method: \"get\",\n    }).then((response) => {\n      this.wordsList = response.data.split(\" \");\n    });\n  },\n  setup: function () {\n    const router = useRouter();\n\n    const goBack = () => {\n      router.go(-2);\n    };\n\n    return { goBack };\n  },\n  data: function () {\n    return {\n      wordsList: [],\n      defaultAccount: null,\n    };\n  },\n  methods: {\n    close: function () {\n      if (confirm(\"SEGURO QUE QUIERES CERRAR? \")) {\n        this.goBack();\n      }\n    },\n  },\n});\n</script>\n\n<style>\n@import \"../styles/style.css\";\n</style>"]}]}