AAWD,KA/DM;AAgEP,IAAA,UAAU,EAAE,sBAAA;AAAA;;AACV,MAAA,cAAc,CAAC,IAAf,GACG,IADH,CACQ,UAAC,WAAD,EAAgB;AACpB;AACA,QAAA,KAAI,CAAC,IAAL,GAAY,WAAW,CAAC,IAAxB;AAEA,QAAA,KAAK,CAAC;AACJ,UAAA,GAAG,EAAE,wCAAwC,KAAI,CAAC,IAD9C;AAEJ,UAAA,MAAM,EAAE;AAFJ,SAAD,CAAL,CAGG,IAHH,CAGQ,UAAC,QAAD,EAAa;AACnB,UAAA,KAAI,CAAC,MAAL,GAAc,QAAQ,CAAC,IAAT,CAAc,CAAd,CAAd;AACA,UAAA,KAAI,CAAC,IAAL,GAAY,QAAQ,CAAC,IAAT,CAAc,CAAd,CAAZ;AACA,UAAA,KAAI,CAAC,OAAL,GAAe,QAAQ,CAAC,IAAT,CAAc,CAAd,CAAf;AACD,SAPD;AAQD,OAbH,EAcG,KAdH,CAcS,UAAC,GAAD,EAAQ;AACb,QAAA,KAAK,CAAC,YAAY,GAAb,CAAL;AACD,OAhBH;AAiBD,KAlFM;AAmFP,IAAA,IAAI,EAAE,gBAAA;AAAA;;AACJ,WAAK,IAAL,GAAY,eAAZ;AAEA,MAAA,KAAK,CAAC;AACJ,QAAA,GAAG,EAAE,wCAAwC,KAAK,IAD9C;AAEJ,QAAA,MAAM,EAAE;AAFJ,OAAD,CAAL,CAGG,IAHH,CAGQ,UAAC,QAAD,EAAa;AACnB,QAAA,MAAI,CAAC,MAAL,GAAc,QAAQ,CAAC,IAAT,CAAc,CAAd,CAAd;AACA,QAAA,MAAI,CAAC,IAAL,GAAY,QAAQ,CAAC,IAAT,CAAc,CAAd,EAAiB,SAAjB,CAA2B,CAA3B,EAA8B,EAA9B,IAAoC,KAAhD;AACD,OAND;AAOD;AA7FM;AAjBI,CAAf","sourcesContent":["\nimport { IonPage, IonHeader, IonToolbar, IonTitle, IonContent } from '@ionic/vue';\n\nimport { FingerprintAIO } from \"@ionic-native/fingerprint-aio\";\nimport { BarcodeScanner } from \"@ionic-native/barcode-scanner\";\nimport { KeychainTouchId } from \"@ionic-native/keychain-touch-id\";\nimport { Camera } from \"@ionic-native/camera\";\n\nimport { FileTransfer} from '@ionic-native/file-transfer';\n\nimport axios from 'axios'\n\nexport default {\n  name: 'HomeFragment',\n  components: { IonHeader, IonToolbar, IonTitle, IonContent, IonPage }\n  data: function () {\n    return {\n      source: \"\",\n      name: \"\",\n      code: \"\",\n      content: \"\",\n      optionsCamera: {\n        quality: 75,\n        destinationType: Camera.DestinationType.FILE_URI,\n        encodingType: Camera.EncodingType.JPEG,\n        mediaType: Camera.MediaType.PICTURE,\n      },\n    };\n  },\n  methods: {\n    photo: function () {\n      Camera.getPicture(this.optionsCamera)\n        .then((imageData) => {\n          // imageData is either a base64 encoded string or a file URI\n          // If it's base64 (DATA_URL):\n\n          const f = FileTransfer.create()\n\n          f.upload(imageData,\"http://192.168.1.41:2999/upload\", {\n            fileKey: 'file',\n            fileName: \"test.jpg\"\n          })\n          .then((data) => {\n            alert(\"Success: \"+ data);\n          });\n\n\n        })\n        .catch((err) => {\n          alert(\"Error: \" + err);\n        });\n    },\n    login: function () {\n      FingerprintAIO.show({\n        title: \"Login\",\n        subtitle: \"Please login to connect to the servers\",\n        description: \"This is authentication\",\n      })\n        .then((result) => {\n          alert(result + \" OK!\");\n        })\n        .catch((err) => {\n          alert(err + \" ERROR!\");\n        });\n    },\n    loginsaver: function () {\n      KeychainTouchId.isAvailable()\n        .then((res) => {\n          const pass = \"123123\";\n\n          KeychainTouchId.save(\"user1\", pass, true)\n            .then((res2) => {\n              alert(\"Login info saved\" + res + \"-\" + res2);\n            })\n            .catch((err2) => {\n              alert(\"Error: \" + err2);\n            });\n        })\n        .catch((err) => alert(\"Error: \" + err));\n    },\n    loginverifier: function () {\n      KeychainTouchId.isAvailable()\n        .then(() => {\n          KeychainTouchId.verify(\"user1\", \"Use to unlock app\")\n            .then((res2) => {\n              alert(\"User: user1 and Password: \" + res2);\n            })\n            .catch((err2) => {\n              alert(\"Error: \" + err2);\n            });\n        })\n        .catch((err) => alert(\"Error: \" + err));\n    },\n    barscanner: function () {\n      BarcodeScanner.scan()\n        .then((barcodeData) => {\n          //alert(\"Barcode data: \" + barcodeData.text);\n          this.code = barcodeData.text;\n\n          axios({\n            url: \"http://192.168.1.41:3013/code?code=\" + this.code,\n            method: \"get\",\n          }).then((response) => {\n            this.source = response.data[2];\n            this.name = response.data[0];\n            this.content = response.data[1];\n          });\n        })\n        .catch((err) => {\n          alert(\"Error: \" + err);\n        });\n    },\n    test: function () {\n      this.code = \"8470001790590\";\n\n      axios({\n        url: \"http://192.168.1.41:3012/code?code=\" + this.code,\n        method: \"get\",\n      }).then((response) => {\n        this.source = response.data[0];\n        this.name = response.data[1].substring(0, 35) + \"...\";\n      });\n    },\n  },\n};\n"],"sourceRoot":""}]}