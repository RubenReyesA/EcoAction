R;AAJc;AAJ2B,iBAAvB,CADb;;AAAA;AACT,qBAAK,QADI;AAAA;AAAA,uBAaI,KAAK,QAAL,CAAc,OAAd,EAbJ;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAF;;AAAA;AAAA;AAAA;;AAAA;AAAA;AA/EF;AA/EoB,CAAD,CAA9B","sourcesContent":["<template>\n  <ion-page>\n    <ion-header :translucent=\"true\">\n      <ion-toolbar>\n        <IonButtons>\n          <ion-back-button :text=\"messages[idioma]['backText']\" />\n        </IonButtons>\n        <ion-title>Producto detectado</ion-title>\n      </ion-toolbar>\n    </ion-header>\n\n    <ion-content :fullscreen=\"true\" class=\"ion-padding\">\n      <ion-card v-if=\"!isLoading\">\n        <ion-slides pager=\"true\" :options=\"sliderOptions\">\n          <ion-slide v-for=\"i in sources\" :key=\"i\">\n            <img :src=\"i\" class=\"imgSlider\" />\n          </ion-slide>\n        </ion-slides>\n        <ion-card-header>\n          <ion-card-subtitle>{{ barcodeNum }}</ion-card-subtitle>\n          <ion-card-title>{{ name }}</ion-card-title>\n        </ion-card-header>\n\n        <ion-card-content>\n          {{ content }}\n        </ion-card-content>\n      </ion-card>\n\n      <div class=\"container2\">\n        <ion-grid>\n          <ion-row>\n            <ion-col>\n              <ion-card\n                class=\"ion-activatable\"\n                @click=\"selectCorrect\"\n                color=\"success\"\n              >\n                <ion-card-content>\n                  <ion-ripple-effect></ion-ripple-effect>\n                  <h1 class=\"btnLabels2 useLondrina\">\n                    Seleccionar como correcto\n                  </h1>\n                </ion-card-content>\n              </ion-card>\n            </ion-col>\n          </ion-row>\n\n          <ion-row>\n            <ion-col>\n              <ion-card class=\"ion-activatable\" @click=\"close\" color=\"danger\">\n                <ion-card-content>\n                  <ion-ripple-effect></ion-ripple-effect>\n                  <h1 class=\"btnLabels2 useLondrina\">Cerrar</h1>\n                </ion-card-content>\n              </ion-card>\n            </ion-col>\n          </ion-row>\n        </ion-grid>\n      </div>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonContent,\n  IonHeader,\n  IonPage,\n  IonTitle,\n  IonToolbar,\n  IonButtons,\n  IonBackButton,\n  IonCard,\n  IonCardContent,\n  IonCardHeader,\n  IonCardSubtitle,\n  IonCardTitle,\n  IonRippleEffect,\n  IonSlides,\n  IonSlide,\n  modalController,\n  IonGrid,\n  IonRow,\n  loadingController,\n  IonCol,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\nimport { NativeStorage } from \"@ionic-native/native-storage\";\n\nimport axios from \"axios\";\n\nimport AIObjectModal from \"../components/AIObjectModal.vue\";\nimport EventBus from \"eventing-bus\";\n\nimport { useRouter } from \"vue-router\";\n\nexport default defineComponent({\n  name: \"BarcodeScannerPage\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonButtons,\n    IonBackButton,\n    IonCard,\n    IonCardContent,\n    IonCardHeader,\n    IonCardSubtitle,\n    IonCardTitle,\n    IonRippleEffect,\n    IonSlides,\n    IonSlide,\n    IonGrid,\n    IonRow,\n    IonCol,\n  },\n  props: {\n    barcodeNum: String,\n  },\n  created: function () {\n    EventBus.on(\"closeModalAI\", () => {\n      if (this.modalBar != undefined) {\n        this.modalBar.dismiss();\n        alert(\"No se ha detectado ningÃºn objeto\");\n      }\n    });\n    EventBus.on(\"closeModalAIwithSuccess\", (id) => {\n      if (this.modalBar != undefined) {\n        this.modalBar.dismiss();\n        this.openItem(id);\n      }\n    });\n  },\n  data: function () {\n    return {\n      messages: {\n        es: require(\"../../public/assets/i18n/es.json\"),\n        ca: require(\"../../public/assets/i18n/ca.json\"),\n        en: require(\"../../public/assets/i18n/en.json\"),\n      },\n      idioma: \"es\",\n      products: require(\"../../public/assets/products.json\"),\n      sources: null,\n      name: null,\n      content: null,\n      control: null,\n      isLoading: true,\n      modalBar: null,\n      sliderOptions: {\n        initialSlide: 0,\n        slidesPerView: 1,\n        autoplay: true,\n      },\n    };\n  },\n  setup: function () {\n    const router = useRouter();\n\n    const openItem = (p) => {\n      router.push({ name: \"residu\", params: { residuID: p } });\n    };\n\n    const goBack = () => {\n      router.go(-1);\n    };\n    return {\n      openItem, goBack\n    };\n  },\n  mounted: function () {\n    NativeStorage.getItem(\"idioma\").then((x) => (this.idioma = x));\n    this.searchProduct();\n  },\n  methods: {\n    searchProduct: function () {\n      /*\n      axios({\n        url: \"http://192.168.1.41:3013/barcode?code=\" + this.barcodeNum,\n        method: \"get\",\n      }).then((response) => {\n        this.sources = response.data[2];\n        this.name = response.data[0];\n        this.content = response.data[1];\n        this.isLoading = false;\n\n        if (this.sourceImg[0] == undefined) {\n          this.close();\n        }\n      });*/\n\n      this.sources = [\n        \"https://tse3.mm.bing.net/th?id=OIP.GwyAviEWsTHRnH1812kQaAHaHa&pid=Api\",\n        \"https://tse3.mm.bing.net/th?id=OIP.nCQGnH_F9cTnE-h-sHaJ4wHaHa&pid=Api\",\n        \"https://tse4.mm.bing.net/th?id=OIP.vDNIkM1xc1M5RiRvZaCN_wHaHa&pid=Api\",\n        \"https://tse3.mm.bing.net/th?id=OIP.YUcvpsTuoAmlCuLZyiU1XQHaEg&pid=Api\",\n      ];\n\n      this.name = \"RBIT REFRESHERS HIERBABUENA 16UN.\";\n      this.content = \"RBIT REFRESHERS HIERBABUENA 16UN.\";\n      this.isLoading = false;\n    },\n    selectCorrect: function () {\n      loadingController\n        .create({\n          message: \"Analizando...\",\n        })\n        .then((loading) => {\n          this.control = loading;\n          loading.present();\n          axios({\n        url:\n          \"http://192.168.1.41:3013/getModalDetailsfromImageURL?url=\" +\n          this.sources[0] +\n          \"&name=\" +\n          this.name +\n          \"&url2=\" +\n          this.sources[1] +\n          \"&url3=\" +\n          this.sources[2] +\n          \"&url4=\" +\n          this.sources[3],\n        method: \"get\",\n      }).then((response) => {\n        const r1 = response.data;\n        const r2 = [];\n        const r3 = [];\n\n        for (let i = 0; i < r1.length; i++) {\n          switch (this.idioma) {\n            case \"es\":\n              r2.push(this.products[r1[i]][\"nameES\"]);\n              break;\n            case \"ca\":\n              r2.push(this.products[r1[i]][\"nameCA\"]);\n              break;\n            case \"en\":\n              r2.push(this.products[r1[i]][\"nameEN\"]);\n              break;\n          }\n          r3.push(this.products[r1[i]][\"imgLinks\"]);\n        }\n\n        //TODO --> SELECT PRODUCT\n        this.openModal(r1, r2, r3);\n        this.control.dismiss();\n      });\n        });\n      \n    },\n    close: function () {\n      this.goBack();\n    },\n    openModal: async function (objectIds, titles, imgPaths) {\n      this.modalBar = await modalController.create({\n        component: AIObjectModal,\n        cssClass: \"my-custom-modal-css\",\n        swipeToClose: true,\n        componentProps: {\n          ids: [1, 2, 3],\n          objectIds,\n          titles,\n          imgPaths,\n        },\n      });\n\n      return await this.modalBar.present();\n    },\n  },\n});\n</script>\n\n<style>\n@import \"../styles/style.css\";\n</style>"],"sourceRoot":""}]}