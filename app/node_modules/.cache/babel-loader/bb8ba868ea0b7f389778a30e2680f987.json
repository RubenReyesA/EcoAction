;AAFnB,oBAAA,QAAQ,EAAR,EAEmB;AADnB,oBAAA,KAAgC,EAAhC;AAAA,+BAAA,KAAA;AAAA,gCAAA;AAAA;AACmB,mBAHrB,CAGqB,EACrB,YAAA,CAGqB,4BAHrB,EAGqB;AAFnB,oBAAA,QAAQ,EAAR,EAEmB;AADnB,oBAAA,KAAgC,EAAhC;AAAA,+BAAA,KAAA;AAAA,gCAAA;AAAA;AACmB,mBAHrB,CADqB,CAHrB;AAAA,iB,CAQgB;;AAAA,eATlB,CAFqB,EAarB,YAAA,CAKmB,2BALnB,EAKmB,IALnB,EAKmB;kCAJjB;AAAA,yBAGqB,CAHrB,YAAA,CAGqB,4BAHrB,EAGqB;AAFnB,oBAAA,QAAQ,EAAR,EAEmB;AADnB,oBAAA,KAAiC,EAAjC;AAAA,+BAAA,KAAA;AAAA,gCAAA;AAAA;AACmB,mBAHrB,CAGqB,CAHrB;AAAA,iB,CAIiB;;AAAA,eALnB,CAbqB,CAHrB;AAAA,a,CAsBS;;AAAA,WAvBX,C,iCAfW,EAwCkB,IAAA,CAAA,S,kBAA7B,YAAA,CAWM,KAXN,EAAA,UAAA,EAWM,CAVJ,YAAA,CAIqB,4BAJrB,EAIqB;AAHnB,YAAA,QAAQ,EAAR,EAGmB;AAFnB,YAAA,KAAK,EAAC,KAEa;AADnB,YAAA,KAAgC,EAAhC;AAAA,uBAAA,KAAA;AAAA,wBAAA;AAAA;AACmB,WAJrB,CAUI,EALJ,YAAA,CAIqB,4BAJrB,EAIqB;AAHnB,YAAA,QAAQ,EAAR,EAGmB;AAFnB,YAAA,KAAK,EAAC,MAEa;AADnB,YAAA,KAAgC,EAAhC;AAAA,uBAAA,KAAA;AAAA,wBAAA;AAAA;AACmB,WAJrB,CAKI,CAXN,C,iCAxCW,CAVX;AAAA,S;;OADF,CAFa,CAPb;AAAA,K,CAyES;;AAAA,GA1EX,C","sourcesContent":["<template>\n  <ion-page>\n    <ion-header :translucent=\"true\">\n      <ion-toolbar>\n        <IonButtons>\n          <ion-back-button :text=\"messages[idioma]['backText']\" />\n        </IonButtons>\n        <ion-title>Código de barras</ion-title>\n      </ion-toolbar>\n    </ion-header>\n\n    <ion-content :fullscreen=\"true\" class=\"ion-padding\">\n      <ion-card v-if=\"!isLoading\">\n        <img :src=\"sourceImg\" style=\"width: 200px; height: 200px\" />\n        <ion-card-header>\n          <ion-card-subtitle>{{ barcodeNum }}</ion-card-subtitle>\n          <ion-card-title>{{ name }}</ion-card-title>\n        </ion-card-header>\n\n        <ion-card-content>\n          {{ content }}\n        </ion-card-content>\n      </ion-card>\n\n      <div class=\"container\" v-if=\"!isLoading\">\n        <ion-button\n          color=\"success\"\n          expand=\"block\"\n          @click=\"selectCorrect\"\n          class=\"btn\"\n          >Seleccionar como correcto</ion-button\n        >\n        <ion-button class=\"btn1\" color=\"danger\" expand=\"block\" @click=\"close\"\n          >Cerrar</ion-button\n        >\n      </div>\n\n      <ion-card v-if=\"isLoading\">\n        <ion-skeleton-text\n          animated\n          style=\"width: 200px; height: 200px\"\n        ></ion-skeleton-text>\n\n        <ion-card-header>\n          <ion-skeleton-text\n            animated\n            style=\"width: 50%; height: 20px\"\n          ></ion-skeleton-text>\n          <ion-skeleton-text\n            animated\n            style=\"width: 60%; height: 30px\"\n          ></ion-skeleton-text>\n        </ion-card-header>\n\n        <ion-card-content>\n          <ion-skeleton-text\n            animated\n            style=\"width: 80%; height: 100px\"\n          ></ion-skeleton-text>\n        </ion-card-content>\n      </ion-card>\n\n      <div class=\"container\" v-if=\"isLoading\">\n        <ion-skeleton-text\n          animated\n          class=\"btn\"\n          style=\"width: 80%; height: 60px\"\n        ></ion-skeleton-text>\n        <ion-skeleton-text\n          animated\n          class=\"btn1\"\n          style=\"width: 80%; height: 60px\"\n        ></ion-skeleton-text>\n      </div>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonContent,\n  IonHeader,\n  IonPage,\n  IonTitle,\n  IonToolbar,\n  IonButtons,\n  IonBackButton,\n  IonSkeletonText,\n  IonCard,\n  IonCardContent,\n  IonCardHeader,\n  IonCardSubtitle,\n  IonCardTitle,\n  IonButton,\n  modalController,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\nimport { NativeStorage } from \"@ionic-native/native-storage\";\n\nimport axios from \"axios\";\n\nimport AIObjectModal from \"../components/AIObjectModal.vue\";\nimport EventBus from \"eventing-bus\";\n\nexport default defineComponent({\n  name: \"BarcodeScannerPage\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonButtons,\n    IonBackButton,\n    IonSkeletonText,\n    IonCard,\n    IonCardContent,\n    IonCardHeader,\n    IonCardSubtitle,\n    IonCardTitle,\n    IonButton,\n  },\n  props: {\n    barcodeNum: String,\n  },\n  created: function () {\n    EventBus.on(\"closeModalAI\", () => {\n      if (this.modalBar != undefined) {\n        this.modalBar.dismiss();\n        alert(\"No se ha detectado ningún objeto\");\n      }\n    });\n    EventBus.on(\"closeModalAIwithSuccess\", (id) => {\n      if (this.modalBar != undefined) {\n        this.modalBar.dismiss();\n        alert(id);\n      }\n    });\n  },\n  data: function () {\n    return {\n      messages: {\n        es: require(\"../../public/assets/i18n/es.json\"),\n        ca: require(\"../../public/assets/i18n/ca.json\"),\n        en: require(\"../../public/assets/i18n/en.json\"),\n      },\n      idioma: \"es\",\n      products: require(\"../../public/assets/products.json\"),\n      sourceImg: null,\n      sources: null,\n      name: null,\n      content: null,\n      isLoading: true,\n      modalBar: null,\n    };\n  },\n  mounted: function () {\n    NativeStorage.getItem(\"idioma\").then((x) => (this.idioma = x));\n    this.searchProduct();\n  },\n  methods: {\n    searchProduct: function () {\n      axios({\n        url: \"http://192.168.1.41:3013/barcode?code=\" + this.barcodeNum,\n        method: \"get\",\n      }).then((response) => {\n        this.sourceImg = response.data[2][0];\n        this.sources = response.data[2];\n        this.name = response.data[0];\n        this.content = response.data[1];\n        this.isLoading = false;\n      });\n    },\n    selectCorrect: function () {\n      axios({\n        url:\n          \"http://192.168.1.41:3013/getModalDetailsfromImageURL?url=\" +\n          this.sources[0] +\n          \"&name=\" +\n          this.name +\n          \"&url2=\" +\n          this.sources[1] +\n          \"&url3=\" +\n          this.sources[2] +\n          \"&url4=\" +\n          this.sources[3],\n        method: \"get\",\n      }).then((response) => {\n        const r1 = response.data;\n        const r2 = [];\n        const r3 = [];\n\n        for (let i = 0; i < r1.length; i++) {\n          switch (this.idioma) {\n            case \"es\":\n              r2.push(this.products[r1[i]][\"nameES\"]);\n              break;\n            case \"ca\":\n              r2.push(this.products[r1[i]][\"nameCA\"]);\n              break;\n            case \"en\":\n              r2.push(this.products[r1[i]][\"nameEN\"]);\n              break;\n          }\n          r3.push(this.products[r1[i]][\"imgLinks\"]);\n        }\n\n        //TODO --> SELECT PRODUCT\n        this.openModal(r1, r2, r3);\n      });\n    },\n    close: function () {\n      prompt(\"GI?\");\n    },\n    openModal: async function (objectIds, titles, imgPaths) {\n      this.modalBar = await modalController.create({\n        component: AIObjectModal,\n        cssClass: \"my-custom-modal-css\",\n        swipeToClose: true,\n        componentProps: {\n          ids: [1, 2, 3],\n          objectIds,\n          titles,\n          imgPaths,\n        },\n      });\n\n      return await this.modalBar.present();\n    },\n  },\n});\n</script>\n\n<style>\n@import \"../styles/homeStyle.css\";\n</style>"],"sourceRoot":""}]}