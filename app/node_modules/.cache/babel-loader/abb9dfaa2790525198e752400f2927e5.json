AAW,EAAX;AAFK,KAAP;AAID;AAjHY,CAAf","sourcesContent":["<template>\n  <div id=\"map\" ref=\"mapDivRef\"></div>\n</template>\n\n<script>\nimport MarkerClusterer from \"@googlemaps/markerclustererplus\";\nimport { ref, onMounted } from \"vue\";\n\nimport EventBus from \"eventing-bus\";\n\nexport default {\n  name: \"GMap\",\n  props: {\n    center: { lat: Number, lng: Number },\n    markers: Array,\n    zoom: Number,\n    mapType: String,\n    disableUI: Boolean,\n  },\n  created: function () {\n    EventBus.on(\"markersUpdate\", this.checkMyMarker);\n  },\n  methods: {\n    checkMyMarker: function () {\n      console.log(33);\n      window.createMyMarker();\n    },\n  },\n  setup(props) {\n    const map = ref(null);\n    const mapDivRef = ref(null);\n\n    onMounted(() => {\n      //const key = \"AIzaSyCX6DY6Q0JYgekyFRtC0r7mrkDDXtxxcSU\";\n      const key = \"\";\n\n      // create the script element to load\n      const googleMapScript = document.createElement(\"SCRIPT\");\n      googleMapScript.setAttribute(\n        \"src\",\n        `https://maps.googleapis.com/maps/api/js?key=${key}&callback=initMap`\n      );\n      googleMapScript.setAttribute(\"defer\", \"\");\n      googleMapScript.setAttribute(\"async\", \"\");\n      document.head.appendChild(googleMapScript);\n    });\n\n    const markersList = [];\n    let myMarker = null;\n\n    window.createMyMarker = () => {\n\n      if(myMarker != null){\n        myMarker.setMap(null);\n      }\n\n      myMarker = new window.google.maps.Marker({\n          position: new window.google.maps.LatLng(\n            props.center.lat,\n            props.center.lng\n          ),\n          map: map.value,\n          icon: {\n            path: window.google.maps.SymbolPath.CIRCLE,\n            scale: 10,\n            fillOpacity: 1,\n            strokeWeight: 2,\n            fillColor: \"#5384ED\",\n            strokeColor: \"#ffffff\",\n          },\n        });\n    }\n\n    window.initMap = () => {\n      map.value = new window.google.maps.Map(mapDivRef.value, {\n        mapTypeId: props.mapType,\n        zoom: props.zoom,\n        disableDefaultUI: props.disableUI,\n        center: props.center,\n      });\n\n      window.a = function () {\n        alert(\"uee\");\n      };\n\n      window.createMyMarker();\n\n      props.markers.forEach((markerInfo) => {\n        const mapMarker = new window.google.maps.Marker({\n          position: new window.google.maps.LatLng(\n            markerInfo.lat,\n            markerInfo.lng\n          ),\n          map: map.value,\n          title: markerInfo.title,\n        });\n\n        mapMarker.infoWindow = new window.google.maps.InfoWindow({\n          content: `<h1 onclick=\"a()\" style=\"color: #5e9ca0; text-align: center;\">${markerInfo.title}</h1>\n<p style=\"text-align: center;\"><span\" style=\"text-decoration: underline;\"><strong>MÁS INFORMACIÓN AQUÍ</strong></span></p>`,\n        });\n\n        markersList.push(mapMarker);\n      });\n\n      markersList.forEach(m => {\n        m.addListener(\"click\", () => {\n          if (m.infoWindow != null) {\n            m.infoWindow.open(map.value, m);\n          }\n        });\n      });\n\n      new MarkerClusterer(map.value, markersList, {\n        imagePath:\n          \"https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/m\",\n      });\n    };\n\n    return {\n      mapDivRef,\n      markersList,\n    };\n  },\n};\n</script>\n\n<style lang=\"css\" scoped>\n#map {\n  width: 100%;\n  height: 57vh;\n  background-color: azure;\n}\n</style>"],"sourceRoot":""}]}