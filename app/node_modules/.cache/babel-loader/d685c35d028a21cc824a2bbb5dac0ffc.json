D,EAAmD;qCAAhC;AAAA,6BAAY,C,UAAA,CAAZ;AAAA,qB,CAAgC;;AAAA,mBAAnD,CAAmD,CAAnD;AAAA,iB,CACgB;;AAAA,eAFlB,CAEkB,EAClB,YAAA,CAamB,2BAbnB,EAamB,IAbnB,EAamB;iCAZjB;AAAA,yBAWW,CAXX,YAAA,CAWW,mBAXX,EAWW,IAXX,EAWW;qCAVT;AAAA,6BAIU,CAJV,YAAA,CAIU,kBAJV,EAIU,IAJV,EAIU;yCAHR;AAAA,iCAEU,CAFV,YAAA,CAEU,kBAFV,EAEU;AAFD,4BAAA,KAAK,EAAC;AAEL,2BAFV,EAAuB;6CACrB;AAAA,qCAAyC,CAAzC,UAAyC,CAAzC;AAAA,6B,CADqB;;AAAA,2BAAvB,CAEU,CAFV;AAAA,yB,CAGQ;;AAAA,uBAJV,CAIU,EACV,YAAA,CAIU,kBAJV,EAIU,IAJV,EAIU;yCAHR;AAAA,iCAEU,CAFV,YAAA,CAEU,kBAFV,EAEU;AAFD,4BAAA,KAAK,EAAC;AAEL,2BAFV,EAA+B;6CAC7B;AAAA,qCAAwD,CAAxD,YAAA,CAAwD,oBAAxD,EAAwD;AAA7C,gCAAA,IAAI,EAAC,MAAwC;4CAAxB,IAAA,CAAA,UAAwB;;yCAAxB,IAAA,CAAA,UAAA,GAAU,M;;AAAc,+BAAxD,E,IAAA,E,CAAA,E,cAAA,CAAwD,CAAxD;AAAA,6B,CAD6B;;AAAA,2BAA/B,CAEU,CAFV;AAAA,yB,CAGQ;;AAAA,uBAJV,CADU,CAJV;AAAA,qB,CAUS;;AAAA,mBAXX,CAWW,CAXX;AAAA,iB,CAYiB;;AAAA,eAbnB,CADkB,CAFlB;AAAA,a,CAiBS;;AAAA,WAlBX,CAFW,EAsBX,YAAA,CAgCM,KAhCN,EAAA,UAAA,EAgCM,CA/BJ,YAAA,CA8BM,KA9BN,EAAA,UAAA,EA8BM,CA7BJ,YAAA,CA4BW,mBA5BX,EA4BW,IA5BX,EA4BW;6BA3BT;AAAA,qBA0BU,CA1BV,YAAA,CA0BU,kBA1BV,EA0BU,IA1BV,EA0BU;iCAzBR;AAAA,yBAWU,CAXV,YAAA,CAWU,kBAXV,EAWU,IAXV,EAWU;qCAVR;AAAA,6BASW,CATX,YAAA,CASW,mBATX,EASW;AART,wBAAA,KAAK,EAAC,0BAQG;AAPT,wBAAA,KAAK,EAAC,SAOG;AANR,wBAAA,OAAK,EAAE,IAAA,CAAA;AAMC,uBATX,E;yCAKE;AAAA,iCAGmB,CAHnB,YAAA,CAGmB,2BAHnB,EAGmB,IAHnB,EAGmB;6CAFjB;AAAA,qCAAuC,CAAvC,YAAA,CAAuC,4BAAvC,CAAuC,EACvC,WADuC,CAAvC;AAAA,6B,CAEiB;;AAAA,2BAHnB,CAGmB,CAHnB;AAAA,yB;;uBALF,E,CAAA,E,WAAA,CASW,CATX;AAAA,qB,CAUQ;;AAAA,mBAXV,CAWU,EACV,YAAA,CAYU,kBAZV,EAYU,IAZV,EAYU;qCAXR;AAAA,6BAUW,CAVX,YAAA,CAUW,mBAVX,EAUW;AATT,wBAAA,KAAK,EAAC,0BASG;AART,wBAAA,KAAK,EAAC,YAQG;AAPR,wBAAA,QAAQ,EAAA,CAAG,IAAA,CAAA,QAOH;AANR,wBAAA,OAAK,EAAE,IAAA,CAAA;AAMC,uBAVX,E;yCAME;AAAA,iCAGmB,CAHnB,YAAA,CAGmB,2BAHnB,EAGmB,IAHnB,EAGmB;6CAFjB;AAAA,qCAAuC,CAAvC,YAAA,CAAuC,4BAAvC,CAAuC,EACvC,WADuC,CAAvC;AAAA,6B,CAEiB;;AAAA,2BAHnB,CAGmB,CAHnB;AAAA,yB;;uBANF,E,CAAA,E,uBAAA,CAUW,CAVX;AAAA,qB,CAWQ;;AAAA,mBAZV,CADU,CAXV;AAAA,iB,CAyBQ;;AAAA,eA1BV,CA0BU,CA1BV;AAAA,a,CA2BS;;AAAA,WA5BX,CA6BI,CA9BN,CA+BI,CAhCN,CAtBW,CAjDX;AAAA,S;;OADF,CAFa,CAPb;AAAA,K,CAmHS;;AAAA,GApHX,C","sourcesContent":["<template>\n  <ion-page>\n    <ion-header :translucent=\"true\">\n      <ion-toolbar>\n        <IonButtons>\n          <ion-back-button :text=\"messages[idioma]['backText']\" />\n        </IonButtons>\n        <ion-title>Enviar tokens</ion-title>\n      </ion-toolbar>\n    </ion-header>\n\n    <ion-content :fullscreen=\"true\" class=\"ion-padding\">\n      <ion-card id=\"sendCard\">\n        <ion-card-header>\n          <ion-card-subtitle>Cantidad</ion-card-subtitle>\n        </ion-card-header>\n        <ion-card-content>\n          <ion-grid>\n            <ion-row style=\"margin-bottom: 2vh\">\n              <ion-col>\n                <ion-segment v-model=\"segmentValue\">\n                  <ion-segment-button value=\"tokens\">\n                    <ion-label>Tokens</ion-label>\n                  </ion-segment-button>\n                  <ion-segment-button value=\"fiat\">\n                    <ion-label>Fiat</ion-label>\n                  </ion-segment-button>\n                </ion-segment>\n              </ion-col>\n            </ion-row>\n            <ion-row>\n              <ion-col class=\"sendCol\">\n                <ion-input\n                  type=\"number\"\n                  v-model=\"tokenInput\"\n                  :disabled=\"disableTokens\"\n                ></ion-input>\n              </ion-col>\n              <ion-col class=\"midCol\"> ---- </ion-col>\n              <ion-col class=\"sendCol\">\n                <ion-input\n                  type=\"number\"\n                  v-model=\"fiatInput\"\n                  :disabled=\"disableFiat\"\n                ></ion-input>\n              </ion-col>\n            </ion-row>\n            <ion-row>\n              <ion-col class=\"midCol\">\n                <p>\n                  Comision fee: {{ comission }} {{ currency.toUpperCase() }}\n                </p>\n              </ion-col>\n            </ion-row>\n            <ion-row>\n              <ion-col class=\"midCol\">\n                <p>Total a pagar: {{ total }} {{ currency.toUpperCase() }}</p>\n              </ion-col>\n            </ion-row>\n          </ion-grid>\n        </ion-card-content>\n      </ion-card>\n\n      <ion-card>\n        <ion-card-header>\n          <ion-card-subtitle>Destinatario</ion-card-subtitle>\n        </ion-card-header>\n        <ion-card-content>\n          <ion-grid>\n            <ion-row>\n              <ion-col class=\"midCol\">\n                <p>Inserte identificador del usuario:</p>\n              </ion-col>\n            </ion-row>\n            <ion-row>\n              <ion-col class=\"midCol sendCol\">\n                <ion-input type=\"text\" v-model=\"identifier\"></ion-input>\n              </ion-col>\n            </ion-row>\n          </ion-grid>\n        </ion-card-content>\n      </ion-card>\n\n      <div class=\"container2\">\n        <div id=\"loginRegisterDiv\">\n          <ion-grid>\n            <ion-row>\n              <ion-col>\n                <ion-card\n                  class=\"btnCard3 ion-activatable\"\n                  color=\"warning\"\n                  @click=\"checkData\"\n                >\n                  <ion-card-content>\n                    <ion-ripple-effect></ion-ripple-effect>\n                    <h1 class=\"btnLabels2 useLondrina\">Verificar</h1>\n                  </ion-card-content>\n                </ion-card>\n              </ion-col>\n              <ion-col>\n                <ion-card\n                  class=\"btnCard3 ion-activatable\"\n                  color=\"lightgreen\"\n                  :disabled=\"!verified\"\n                  @click=\"updateTokens\"\n                >\n                  <ion-card-content>\n                    <ion-ripple-effect></ion-ripple-effect>\n                    <h1 class=\"btnLabels2 useLondrina\">Confirmar</h1>\n                  </ion-card-content>\n                </ion-card>\n              </ion-col>\n            </ion-row>\n          </ion-grid>\n        </div>\n      </div>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonContent,\n  IonHeader,\n  IonPage,\n  IonTitle,\n  IonToolbar,\n  IonButtons,\n  IonBackButton,\n  IonGrid,\n  IonRow,\n  IonCol,\n  IonCard,\n  IonCardContent,\n  IonInput,\n  IonSegment,\n  loadingController,\n  IonLabel,\n  IonCardHeader,\n  IonCardSubtitle,\n  IonRippleEffect,\n  IonSegmentButton,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\nimport { NativeStorage } from \"@ionic-native/native-storage\";\nimport axios from \"axios\";\nimport {sha224} from \"js-sha256\";\n\nexport default defineComponent({\n  name: \"SendPage\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonButtons,\n    IonBackButton,\n    IonGrid,\n    IonRow,\n    IonCol,\n    IonCard,\n    IonCardContent,\n    IonInput,\n    IonSegment,\n    IonLabel,\n    IonCardHeader,\n    IonRippleEffect,\n    IonCardSubtitle,\n    IonSegmentButton,\n  },\n  props: ['b', 't', 'm'],\n  data: function () {\n    return {\n      messages: {\n        es: require(\"../../public/assets/i18n/es.json\"),\n        ca: require(\"../../public/assets/i18n/ca.json\"),\n        en: require(\"../../public/assets/i18n/en.json\"),\n      },\n      idioma: \"es\",\n      currency: \"eur\",\n      tokenInput: 0,\n      fiatInput: 0,\n      changePrice: \"\",\n      controlInfo: null,\n      controlSend:null,\n      segmentValue: \"tokens\",\n      disableTokens: false,\n      disableFiat: true,\n      identifier: \"\",\n      comission: \"0\",\n      total: \"0\",\n      verified: false,\n      correctInput: true,\n      balance: 0,\n      checker: [\n        \"T\",\n        \"R\",\n        \"W\",\n        \"A\",\n        \"G\",\n        \"M\",\n        \"Y\",\n        \"F\",\n        \"P\",\n        \"D\",\n        \"X\",\n        \"B\",\n        \"N\",\n        \"J\",\n        \"Z\",\n        \"S\",\n        \"Q\",\n        \"V\",\n        \"H\",\n        \"L\",\n        \"C\",\n        \"K\",\n        \"E\",\n      ],\n    };\n  },\n  methods: {\n    checkIdentifier: function (id) {\n      const num = parseInt(id.substring(0, id.length - 1));\n      const letter = id.substring(id.length - 1, id.length);\n\n      const r = num % 23;\n\n      return (\n        this.checker[r] == letter.toLowerCase() ||\n        this.checker[r] == letter.toUpperCase()\n      );\n    },\n    checkData: function () {\n      if (this.correctInput) {\n        if (this.checkIdentifier(this.identifier)) {\n          axios({\n            url: \"http://192.168.1.41:3013/checkID?id=\" + this.identifier,\n            method: \"get\",\n          })\n            .then((response) => {\n              if (response.data == true) {\n                this.verified = true;\n              }\n            })\n            .catch((err) => {\n              console.log(\"Error => \" + err);\n            });\n        } else {\n          alert(\"Error: DNI inválido\");\n        }\n      }\n    },\n    updateTokens: function () {\n      const pass = prompt(\n        \"Introduzca su contraseña para autorizar el envio de tokens\"\n      );\n\n      if (pass != null && pass != \"\") {\n        loadingController\n          .create({\n            message:\n              \"Añadiendo tokens. Por favor espere y no cierre la aplicación\",\n          })\n          .then((loading) => {\n            this.controlSend = loading;\n            loading.present();\n            axios({\n              url: \"http://192.168.1.41:3013/updateTokens\",\n              method: \"post\",\n              data: [\n                this.identifier,\n                this.m,\n                sha224(pass),\n                this.tokenInput,\n              ],\n            }).then((response) => {\n              this.controlSend.dismiss();\n              alert(response.data);\n            });\n          })\n          .catch((err) => {\n            this.controlSend.dismiss();\n            alert(\"Se ha producido un error: \" + err);\n          });\n      }\n    },\n  },\n  watch: {\n    segmentValue: function () {\n      if (this.segmentValue == \"fiat\") {\n        this.disableTokens = true;\n        this.disableFiat = false;\n      } else {\n        this.disableTokens = false;\n        this.disableFiat = true;\n      }\n    },\n    tokenInput: function () {\n      if (this.disableFiat) {\n        this.fiatInput =\n          Math.round(this.tokenInput * 0.00001 * this.changePrice * 100) / 100;\n      }\n\n      if (this.tokenInput != \"\") {\n        if (parseFloat(this.tokenInput) <= parseFloat(this.balance)) {\n          axios({\n            url:\n              \"http://192.168.1.41:3013/getGasEstimation?method=send&addr=\" + this.t + \"&q=\" +\n              this.tokenInput +\n              \"&currency=\" +\n              this.currency,\n            method: \"get\",\n          })\n            .then((response2) => {\n              this.comission = response2.data;\n              this.total =\n                parseFloat(this.fiatInput) + parseFloat(this.comission);\n            })\n            .catch((err) => {\n              console.log(\"Error => \" + err);\n            });\n        } else {\n          alert(\"Insufficient balance\");\n        }\n      }\n    },\n    fiatInput: function () {\n      if (this.disableTokens) {\n        this.tokenInput = Math.round(\n          this.fiatInput / this.changePrice / 0.00001\n        );\n      }\n    },\n  },\n  mounted: async function () {\n    NativeStorage.getItem(\"idioma\").then((x) => (this.idioma = x));\n    await NativeStorage.getItem(\"currency\").then((x) => (this.currency = x));\n\n    loadingController\n      .create({\n        message: \"Cargando información...\",\n      })\n      .then((loading) => {\n        this.controlInfo = loading;\n        loading.present();\n        axios({\n          url: \"http://192.168.1.41:3013/getEthPrice?currency=\" + this.currency,\n          method: \"get\",\n        })\n          .then((response) => {\n            this.changePrice = parseFloat(response.data);\n\n            axios({\n              url:\n                \"http://192.168.1.41:3013/getGasEstimation?method=send&addr=\" + this.t + \"&q=\" +\n                this.tokenInput +\n                \"&currency=\" +\n                this.currency,\n              method: \"get\",\n            })\n              .then((response2) => {\n                this.comission = response2.data;\n                this.total =\n                  parseFloat(this.fiatInput) + parseFloat(this.comission);\n\n                axios({\n                  url:\n                    \"http://192.168.1.41:3013/getBalance?tokenAddress=\" + this.t +\n                    \"&currency=\" +\n                    this.currency,\n                  method: \"get\",\n                })\n                  .then((response3) => {\n                    this.balance = response3.data[0];\n                    this.controlInfo.dismiss();\n                  })\n                  .catch((err) => {\n                    console.log(\"Error => \" + err);\n                    this.controlInfo.dismiss();\n                  });\n              })\n              .catch((err) => {\n                console.log(\"Error => \" + err);\n                this.controlInfo.dismiss();\n              });\n          })\n          .catch((err) => {\n            console.log(\"Error => \" + err);\n            this.controlInfo.dismiss();\n          });\n      });\n  },\n});\n</script>\n\n<style scoped>\n@import \"../styles/style.css\";\n@import \"../styles/fontStyle.css\";\n@import \"../styles/colorStyle.css\";\n</style>"],"sourceRoot":""}]}