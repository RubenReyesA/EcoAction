nt":["<template>\n  <ion-page>\n    <ion-header>\n      <ion-toolbar>\n        <ion-title>{{messages[idioma][\"searchTab\"]}}</ion-title>\n      </ion-toolbar>\n    </ion-header>\n\n    <ion-content :fullscreen=\"true\">\n      <ion-header collapse=\"condense\">\n        <ion-toolbar>\n          <ion-title size=\"large\">{{messages[idioma][\"searchTab\"]}}</ion-title>\n        </ion-toolbar>\n      </ion-header>\n\n      <div class=\"container2\">\n        <img id=\"searchImage\" src=\"../../public/assets/images/search.png\" />\n\n        <ion-grid>\n          <ion-row>\n            <ion-col>\n              <ion-card\n                class=\"btnCard4 ion-activatable\"\n                @click=\"barscanner\"\n                color=\"lightred\"\n              >\n                <ion-card-content>\n                  <ion-ripple-effect></ion-ripple-effect>\n                  <div class=\"container1_2\">\n                    <ion-icon\n                      :icon=\"barcode\"\n                      class=\"iconSearch\"\n                      size=\"large\"\n                    ></ion-icon>\n                    <h1 class=\"btnLabels3 useLondrina\">\n                      {{messages[idioma][\"searchBarcode\"]}}\n                    </h1>\n                  </div>\n                </ion-card-content>\n              </ion-card>\n            </ion-col>\n          </ion-row>\n\n          <ion-row>\n            <ion-col>\n              <ion-card\n                class=\"btnCard4 ion-activatable\"\n                @click=\"barscanner\"\n                color=\"lightgreen\"\n              >\n                <ion-card-content>\n                  <ion-ripple-effect></ion-ripple-effect>\n                  <div class=\"container1_2\">\n                    <ion-icon\n                      :icon=\"camera\"\n                      class=\"iconSearch\"\n                      size=\"large\"\n                    ></ion-icon>\n                    <h1 class=\"btnLabels3 useLondrina\">\n        