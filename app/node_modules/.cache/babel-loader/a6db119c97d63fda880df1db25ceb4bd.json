ods: {\n    startRegister: function () {\n      alert(\"[TODO] Registro realizado con Ã©xito\");\n    },\n    a: function () {\n      alert(this.inputText.normalize(\"NFD\").replace(/[\\u0300-\\u036f]/g, \"\"));\n    },\n  },\n  setup: function(){\n    return{\n      trashBin,\n    }\n  }\n});\n</script>\n\n<style>\n@import \"../styles/homeStyle.css\";\n@import \"../styles/colorStyle.css\";\n</style>"],"sourceRoot":""}]}