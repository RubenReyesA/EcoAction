    </ion-card-content>\n      </ion-card>\n\n      <ion-segment v-model=\"type\" id=\"segmentBar\">\n        <ion-segment-button value=\"list\">\n          <ion-label>List</ion-label>\n        </ion-segment-button>\n        <ion-segment-button value=\"map\">\n          <ion-label>Map</ion-label>\n        </ion-segment-button>\n      </ion-segment>\n\n      <div v-if=\"type == 'list'\" class=\"animate__animated animate__fadeIn\">\n        <ion-toolbar>\n          <ion-searchbar\n            animated\n            placeholder=\"Search for deixalleries\"\n            inputmode=\"search\"\n            id=\"searchBar\"\n            v-model=\"inputText\"\n          ></ion-searchbar>\n        </ion-toolbar>\n\n        <ion-list>\n          <ion-item\n            button\n            @click=\"a(point[0])\"\n            v-for=\"point in filteredItems\"\n            :key=\"point[0]\"\n          >\n            <ion-label>{{ point[0] }}</ion-label>\n            <ion-note v-if=\"usedGPS\" slot=\"end\">{{ point[9] }}</ion-note>\n          </ion-item>\n        </ion-list>\n      </div>\n\n      <div v-if=\"type == 'map'\" class=\"animate__animated animate__fadeIn\">\n        <g-map\n          :disableUI=\"true\"\n          :zoom=\"12\"\n          mapType=\"roadmap\"\n          :center=\"mapCenter\"\n          :markers=\"markers\"\n        >\n        </g-map>\n      </div>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonContent,\n  IonHeader,\n  IonPage,\n  IonTitle,\n  IonToolbar,\n  IonButtons,\n  IonBackButton,\n  IonCard,\n  IonCardContent,\n  IonSegment,\n  IonSegmentButton,\n  IonLabel,\n  IonSearchbar,\n  IonList,\n  IonItem,\n  IonNote,\n  IonToggle,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\n\nimport { Geolocation } from \"@ionic-native/geolocation\";\nimport { NativeStorage } from \"@ionic-native/native-storage\";\n\nimport EventBus from \"eventing-bus\";\n\nimport GMap from \"./GMap\";\n\nexport default defineComponent({\n  name: \"InfoRecyclingPoints\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonButtons,\n    IonBackButton,\n    IonCard,\n    IonCardContent,\n    IonSegment,\n    IonSegmentButton,\n    IonLabel,\n    IonSearchbar,\n    IonList,\n    IonItem,\n    IonNote,\n    IonToggle,\n    GMap,\n  },\n  data: function () {\n    return {\n      messages: {\n        es: require(\"../../public/assets/i18n/es.json\"),\n        ca: require(\"../../public/assets/i18n/ca.json\"),\n        en: require(\"../../public/assets/i18n/en.json\"),\n      },\n      idioma: \"es\",\n      inputText: \"\",\n      usedGPS: false,\n      points: require(\"../../public/assets/deixalleries.json\"),\n      type: \"map\",\n      markers: null,\n      mapCenter: { lat: 41.3879, lng: 2.16992 },\n    };\n  },\n  watch: {\n    usedGPS: function () {\n      if (this.usedGPS) {\n        this.lookPos();\n      } else {\n        this.mapCenter.lat = 41.3879;\n        this.mapCenter.lng = 2.16992;\n        EventBus.publish(\"markersUpdate\");\n      }\n    },\n  },\n  methods: {\n    generateMarkers: function () {\n      this.markers = [];\n\n      for (let i = 0; i < 5; i++) {\n        if (this.points[i][7][0] != null) {\n          this.markers.push({\n            lat: this.points[i][7][0],\n            lng: this.points[i][7][1],\n            title: this.points[i][0],\n          });\n        }\n      }\n\n      /*this.markers=[{\n        lat: 41.3879,\n        lng: 2.16992,\n        title: \"Barcelona\"\n      },{\n        lat: 41.3523,\n        lng: 2.1422,\n        title: \"TR\"\n      }\n      ]*/\n    },\n    a: function (f) {\n      alert(f);\n    },\n    lookPos: function () {\n      Geolocation.getCurrentPosition().then((resp) => {\n        this.mapCenter.lat = resp.coords.latitude;\n        this.mapCenter.lng = resp.coords.longitude;\n        this.usedGPS = true;\n        EventBus.publish(\"markersUpdate\");\n\n        for (let i = 0; i < this.points.length; i++) {\n          if (this.points[i][7][0] == null) {\n            this.points[i].push(\"MÃ²bil\");\n          } else {\n            this.points[i].push(\n              this.calculateDistance(\n                this.mapCenter.lng,\n                this.points[i][7][1],\n                this.mapCenter.lat,\n                this.points[i][7][0]\n              ) + \" km\"\n            );\n          }\n        }\n      });\n    },\n    calculateDistance: function (lon1, lon2, lat1, lat2) {\n      const p = 0.017453292519943295;\n      const c = Math.cos;\n      const a =\n        0.5 -\n        c((lat1 - lat2) * p) / 2 +\n        (c(lat2 * p) * c(lat1 * p) * (1 - c((lon1 - lon2) * p))) / 2;\n      const dis = 12742 * Math.asin(Math.sqrt(a));\n      return Math.trunc(dis);\n    },\n  },\n  computed: {\n    filteredItems: function () {\n      return this.points.filter((item) => {\n        return (\n          item[0]\n            .toLowerCase()\n            .normalize(\"NFD\")\n            .replace(/[\\u0300-\\u036f]/g, \"\")\n            .indexOf(\n              this.inputText\n                .toLowerCase()\n                .normalize(\"NFD\")\n                .replace(/[\\u0300-\\u036f]/g, \"\")\n            ) > -1\n        );\n      });\n    },\n  },\n  mounted: function () {\n    NativeStorage.getItem(\"idioma\").then((x) => (this.idioma = x));\n    this.generateMarkers();\n  },\n});\n</script>\n\n<style>\n@import \"../styles/infoStyle.css\";\n@import \"~animate.css/animate.min.css\";\n</style>"],"sourceRoot":""}]}