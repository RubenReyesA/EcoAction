A9B,CAOa,EAEb,YAAA,CAyCc,sBAzCd,EAyCc;AAzCA,QAAA,UAAU,EAAE,IAyCZ;AAzCkB,QAAA,KAAK,EAAC;AAyCxB,OAzCd,E;0BACE;AAAA,iBAiBW,CAjBX,YAAA,CAiBW,mBAjBX,EAiBW,IAjBX,EAiBW;8BAhBT;AAAA,qBAEkB,CAFlB,YAAA,CAEkB,0BAFlB,EAEkB,IAFlB,EAEkB;kCADhB;AAAA,yBAA2C,CAA3C,YAAA,CAA2C,yBAA3C,EAA2C,IAA3C,EAA2C;sCAA3B;AAAA,6BAAU,C,UAAA,CAAV;AAAA,qB,CAA2B;;;;AAAA,mBAA3C,CAA2C,CAA3C;AAAA,iB,CACgB;;;;AAAA,eAFlB,CAEkB,EAClB,YAAA,CAYmB,2BAZnB,EAYmB,IAZnB,EAYmB;kCAXjB;AAAA,yBAAyB,CAAzB,YAAA,CAAyB,GAAzB,EAAyB,IAAzB,EAAG,cAAS,gBAAA,CAAG,IAAA,CAAA,GAAH,CAAZ,EAAkB;AAAA;AAAlB,mBAAyB,EACzB,YAAA,CAA0B,GAA1B,EAA0B,IAA1B,EAAG,eAAU,gBAAA,CAAG,IAAA,CAAA,GAAH,CAAb,EAAmB;AAAA;AAAnB,mBADyB,EAEzB,YAAA,CAA+C,qBAA/C,EAA+C;AAAlC,oBAAA,OAAK,EAAE,IAAA,CAAA;AAA2B,mBAA/C,EAA2B;sCAAE;AAAA,6BAAK,C,UAAA,CAAL;AAAA,qB,CAAF;;;;AAAA,mBAA3B,E;;AAAA,oB,WAAA,CAFyB,EAIZ,IAAA,CAAA,O,kBAAb,YAAA,CAMQ,gBANR,EAMQ;0BAAA;AALL,oBAAA,SAAS,EAAE,IAKN;AAJL,oBAAA,IAAI,EAAE,EAID;AAHN,oBAAA,OAAO,EAAC,SAGF;AAFL,oBAAA,MAAM,EAAA;AAAA,sBAAA,GAAA,EAAQ,IAAA,CAAA,GAAR;AAAW,sBAAA,GAAA,EAAM,IAAA,CAAA;AAAjB;AAED,mBANR,E,IAAA,E;;AAAA,oB,UAAA,C,qCAJyB,CAAzB;AAAA,iB,CAWiB;;;;AAAA,eAZnB,CADkB,CAFlB;AAAA,a,CAgBS;;;;AAAA,WAjBX,CAiBW,EAEX,mBAAA,CAAA,klBAAA,CAFW,CAjBX;AAAA,S;;;;OADF,CAFa,CAPb;AAAA,K,CAmDS;;;;AAAA,GApDX,C","sourcesContent":["<template>\n  <ion-page>\n    <ion-header :translucent=\"true\">\n      <ion-toolbar>\n        <IonButtons>\n          <ion-back-button :text=\"messages[idioma]['backText']\" />\n        </IonButtons>\n        <ion-title>Info Recycling Points</ion-title>\n      </ion-toolbar>\n    </ion-header>\n\n    <ion-content :fullscreen=\"true\" class=\"ion-padding\">\n      <ion-card>\n        <ion-card-header>\n          <ion-card-title>Uso de GPS</ion-card-title>\n        </ion-card-header>\n        <ion-card-content>\n          <p>Latitud: {{ ltl }}</p>\n          <p>Longitud: {{ lon }}</p>\n          <ion-button @click=\"lookPos\">CHECK</ion-button>\n\n          <g-map v-if=\"usedGPS\"\n            :disableUI=\"true\"\n            :zoom=\"12\"\n            mapType=\"roadmap\"\n            :center=\"{lat: ltl, lng:lon}\"\n          >\n          </g-map>\n        </ion-card-content>\n      </ion-card>\n\n      <!--<ion-toolbar>\n        <ion-searchbar\n          animated\n          placeholder=\"Search for deixalleries\"\n          inputmode=\"search\"\n          id=\"searchBar\"\n          v-model=\"inputText\"\n        ></ion-searchbar>\n      </ion-toolbar>\n\n      <ion-list>\n        <ion-item\n          button\n          @click=\"a(point[0])\"\n          v-for=\"point in filteredItems\"\n          :key=\"point[0]\"\n        >\n          <ion-label>{{ point[0] }}</ion-label>\n          <ion-note v-if=\"usedGPS\" slot=\"end\">{{point[9]}}</ion-note>\n        </ion-item>\n      </ion-list> <!-->\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport {\n  IonContent,\n  IonHeader,\n  IonPage,\n  IonTitle,\n  IonToolbar,\n  IonButtons,\n  IonButton,\n  IonBackButton,\n  IonCard,\n  IonCardHeader,\n  IonCardTitle,\n  IonCardContent,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\n\nimport { Geolocation } from \"@ionic-native/geolocation\";\nimport { NativeStorage } from \"@ionic-native/native-storage\";\nimport GMap from \"./GMap\";\n\nexport default defineComponent({\n  name: \"InfoRecyclingPoints\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonButtons,\n    IonBackButton,\n    IonButton,\n    IonCard,\n    IonCardHeader,\n    IonCardTitle,\n    IonCardContent,\n    GMap,\n  },\n  data: function () {\n    return {\n      messages: {\n        es: require(\"../../public/assets/i18n/es.json\"),\n        ca: require(\"../../public/assets/i18n/ca.json\"),\n        en: require(\"../../public/assets/i18n/en.json\"),\n      },\n      idioma: \"es\",\n      inputText: \"\",\n      usedGPS: false,\n      ltl: \"\",\n      lon: \"\",\n      points: require(\"../../public/assets/deixalleries.json\"),\n    };\n  },\n  methods: {\n    a: function (f) {\n      alert(f);\n    },\n    lookPos: function () {\n      Geolocation.getCurrentPosition().then((resp) => {\n        this.ltl = resp.coords.latitude;\n        this.lon = resp.coords.longitude;\n        this.usedGPS = true;\n\n        for (let i = 0; i < this.points.length; i++) {\n          if (this.points[i][7][0] == null) {\n            this.points[i].push(\"Mòbil\");\n          } else {\n            this.points[i].push(\n              this.calculateDistance(\n                this.lon,\n                this.points[i][7][1],\n                this.ltl,\n                this.points[i][7][0]\n              ) + \" km\"\n            );\n          }\n        }\n      });\n    },\n    calculateDistance: function (lon1, lon2, lat1, lat2) {\n      const p = 0.017453292519943295;\n      const c = Math.cos;\n      const a =\n        0.5 -\n        c((lat1 - lat2) * p) / 2 +\n        (c(lat2 * p) * c(lat1 * p) * (1 - c((lon1 - lon2) * p))) / 2;\n      const dis = 12742 * Math.asin(Math.sqrt(a));\n      return Math.trunc(dis);\n    },\n  },\n  computed: {\n    filteredItems: function () {\n      return this.points.filter((item) => {\n        return (\n          item[0]\n            .toLowerCase()\n            .normalize(\"NFD\")\n            .replace(/[\\u0300-\\u036f]/g, \"\")\n            .indexOf(\n              this.inputText\n                .toLowerCase()\n                .normalize(\"NFD\")\n                .replace(/[\\u0300-\\u036f]/g, \"\")\n            ) > -1\n        );\n      });\n    },\n  },\n  mounted: function () {\n    NativeStorage.getItem(\"idioma\").then((x) => (this.idioma = x));\n  },\n});\n</script>\n\n<style>\n@import \"../styles/infoStyle.css\";\n</style>"],"sourceRoot":""}]}